    "queryUnits": [
        {
            "mainTableName": "clinical_lab_recorded",
            "mainTableAlias": "clr",
            "groupBy": false,
            "joinTables": [
                {
                    "alias": "clv",
                    "name": "clinical_lab_values_ext_view_crosstab",
                    "type": "LEFT",
                    "expression": "ON (clv.clinical_lab_recorded_id = clr.clinical_lab_recorded_id)"
                },
                {
                    "alias": "pd",
                    "dependsOn": "clr",
                    "name": "patient_details",
                    "type": "LEFT",
                    "expression": "ON pd.mr_no = clr.mrno"
                },
                {
                    "alias": "pr",
                    "dependsOn": "pd",
                    "name": "patient_registration",
                    "type": "Left",
                    "expression": "ON pr.patient_id = coalesce(pd.visit_id,pd.previous_visit_id)"
                },
                {
                    "name": "hospital_center_master",
                    "alias": "hcm",
                    "dependsOn": "pr",
                    "expression": "ON (hcm.center_id = pr.center_id)"
                },
                {
                    "name": "country_master",
                    "alias": "hc",
                    "dependsOn": "hcm",
                    "expression": " ON hcm.country_id = hc.country_id"
                },
                {
                    "name": "city",
                    "alias": "hci",
                    "dependsOn": "hcm",
                    "expression": " ON hcm.city_id= hci.city_id"
                },
                {
                    "name": "state_master",
                    "alias": "hst",
                    "dependsOn": "hcm",
                    "expression": " ON  hcm.state_id= hst.state_id"
                },
                {
                    "name": "region_master",
                    "alias": "hrm",
                    "dependsOn": "hcm",
                    "expression": " ON hcm.region_id = hrm.region_id"
                },
                {
                    "isInclude": true,
                    "includeName": "PatientDetailFields.srjs"
                }
            ]
        }
    ],
    "includes": [
        "PatientDetailFields.srjs"
    ],
    "reportGroup": "Clinical Data",
    "title": "Clinical Lab Report"
}