        "discount_auth_hosp_name": {
            "allowHorizGroup": true,
            "allowNull": true,
            "allowedValuesQuery": "SELECT disc_auth_name AS discount_auth_hosp_name FROM discount_authorizer GROUP BY disc_auth_name",
            "dataType": "string",
            "displayName": "Hospital Discount Authorizer",
            "expression": "dah.disc_auth_name ",
            "filterable": true,
            "groupable": true,
            "name": "discount_auth_hosp_name",
            "table": "dah",
