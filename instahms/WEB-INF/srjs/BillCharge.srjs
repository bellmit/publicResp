    "queryUnits": [
        {
            "joinTables": [
                {
                    "alias": "b",
                    "dependsOn": "bc",
                    "expression": "ON (bc.bill_no = b.bill_no)",
                    "name": "bill",
                    "type": "JOIN"
                },
                {
                    "alias": "pbcl",
                    "dependsOn": "b",
                    "expression": "ON (pbcl.bill_no = b.bill_no AND pbcl.priority = 1) ",
                    "name": "bill_claim",
                    "type": "LEFT"
                },
                {
                    "alias": "sbcl",
                    "dependsOn": "b",
                    "expression": "ON (sbcl.bill_no = b.bill_no AND sbcl.priority = 2) ",
                    "name": "bill_claim",
                    "type": "LEFT"
                },
                {
                    "alias": "pbccl",
                    "dependsOn": "bc,pbcl",
                    "expression": "ON (pbccl.charge_id = bc.charge_id AND pbccl.claim_id = pbcl.claim_id) ",
                    "name": "bill_charge_claim",
                    "type": "LEFT"
                },
                {
                    "alias": "sbccl",
                    "dependsOn": "bc,sbcl",
                    "expression": "ON (sbccl.charge_id = bc.charge_id AND sbccl.claim_id = sbcl.claim_id) ",
                    "name": "bill_charge_claim",
                    "type": "LEFT"
                },
                {
                    "alias": "chc",
                    "dependsOn": "bc",
                    "expression": "ON (chc.chargehead_id = bc.charge_head)",
                    "name": "chargehead_constants",
                    "type": "JOIN"
                },
                {
                    "alias": "cgc",
                    "dependsOn": "chc",
                    "expression": "ON (cgc.chargegroup_id = chc.chargegroup_id)",
                    "name": "chargegroup_constants",
                    "type": "JOIN"
                },
                {
                    "alias": "ct",
                    "dependsOn": "bc",
                    "expression": "on (bc.consultation_type_id=ct.consultation_type_id )",
                    "name": "consultation_types",
                    "type": "LEFT"
                },
                {
                    "alias": "ssg",
                    "dependsOn": "bc",
                    "expression": "ON (ssg.service_sub_group_id = bc.service_sub_group_id)",
                    "name": "service_sub_groups",
                    "type": "LEFT"
                },
                {
                    "alias": "sg",
                    "dependsOn": "bc,ssg",
                    "expression": "ON (sg.service_group_id = ssg.service_group_id)",
                    "name": "service_groups",
                    "type": "LEFT"
                },
                {
                    "alias": "bahc",
                    "dependsOn": "chc",
                    "expression": "ON (bahc.account_head_id = chc.account_head_id)",
                    "name": "bill_account_heads",
                    "type": "JOIN"
                },
                {
                    "alias": "sbah",
                    "dependsOn": "ssg",
                    "expression": "ON (sbah.account_head_id = ssg.account_head_id)",
                    "name": "bill_account_heads"
                },
                {
                    "alias": "bcagm",
                    "dependsOn": "bc",
                    "expression": "ON (bcagm.account_group_id = bc.account_group)",
                    "name": "account_group_master",
                    "type": "LEFT"
                },
                {
                    "alias": "tdep",
                    "dependsOn": "bc",
                    "expression": "ON (tdep.dept_id = bc.act_department_id)",
                    "name": "treating_departments_view",
                    "type": "LEFT"
                },
                {
                    "alias": "cdoc",
                    "dependsOn": "bc",
                    "expression": "ON(cdoc.doctor_id=bc.payee_doctor_id)",
                    "name": "doctors",
                    "type": "LEFT"
                },
                {
                    "alias": "predoc",
                    "dependsOn": "bc",
                    "expression": "ON(predoc.doctor_id=bc.prescribing_dr_id)",
                    "name": "doctors",
                    "type": "LEFT"
                },
                {
                    "alias": "dac",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_dr = dac.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "dap",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_pres_dr = dap.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "dar",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_ref = dar.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "dah",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_hosp = dah.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "daov",
                    "dependsOn": "bc",
                    "expression": "ON (bc.overall_discount_auth = daov.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "pamc",
                    "dependsOn": "bc",
                    "expression": "ON (pamc.prior_auth_mode_id = bc.prior_auth_mode_id)",
                    "name": "prior_auth_modes",
                    "type": "LEFT"
                },
                {
                    "alias": "rdept",
                    "dependsOn": "bc",
                    "expression": "ON (bc.revenue_department_id = rdept.dept_id)",
                    "name": "department",
                    "type": "LEFT"
                },
                {
                    "alias": "bctxn",
                    "dependsOn": "bc",
                    "expression": "ON (bc.charge_id = bctxn.bill_charge_id)",
                    "name": "bill_charge_transaction",
                    "type": "LEFT"
                }
            ],
            "mainTableAlias": "bc",
            "mainTableName": "bill_charge",
            "whereExpression": "b.status != 'X' AND bc.status != 'X'",
            "whereTable": "b"
        }
    ],
    "reportGroup": "Billing Reports",
    "title": "Bill Charge Report"
}