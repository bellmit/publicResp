        },
        "bill_patient_full_name": {
            "allowHorizGroup": true,
            "allowNull": true,
            "dataType": "string",
            "description": "Patient or retail customer name",
            "displayName": "Patient Name",
            "expression": "COALESCE (smb.salutation || ' ' || pd.patient_name           || case when coalesce(pd.middle_name, '') = '' then '' else (' ' || pd.middle_name) end           || case when coalesce(pd.last_name, '') = '' then '' else (' ' || pd.last_name) end,        prc.customer_name, isr.patient_name) ",
            "filterable": false,
            "groupable": true,
            "name": "bill_patient_full_name",
            "table": "smb,pr,prc,isr",
            "width": 150
        },
        "orig_rate": {
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "amount",
            "description": "Original Rate ",
            "displayName": "Original Rate",
            "filterable": true,
            "groupable": false,
            "name": "orig_rate",
            "table": "bc",
            "width": 60
        },
