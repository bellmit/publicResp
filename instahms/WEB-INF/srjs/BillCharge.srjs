            "displayName": "Item Prior Auth Mode",
            "expression": "pamc.prior_auth_mode_name ",
            "filterable": false,
            "groupable": false,
            "name": "charge_prior_auth_mode_name",
            "table": "pamc",
            "width": 70
        },
        "amount": {
            "aggFunction": "sum",
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "amount",
            "displayName": "Item Amount w/o Taxes",
            "expression": "(bc.amount) ",
            "filterable": true,
            "groupable": false,
            "name": "amount",
            "table": "bc",
            "width": 60
        },
        "gross_amount": {
            "aggFunction": "sum",
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "amount",
            "displayName": "Gross Amount",
            "expression": "(bc.amount+COALESCE(bc.tax_amt,0) +bc.discount)",
            "filterable": true,
            "groupable": false,
            "name": "gross_amount",
            "table": "bc",
            "width": 60
        },
        "pri_denial_code": {
            "allowHorizGroup": true,
            "allowNull": true,
            "allowedValuesQuery": "SELECT denial_code FROM insurance_denial_codes",
            "dataType": "string",
            "displayName": "Pri. Denial Code",
            "filterable": true,
            "groupable": true,
            "expression": "pbccl.denial_code",
            "table": "pbccl",
            "width": 80
        },
        "sec_denial_code": {
            "allowHorizGroup": true,
            "allowNull": true,
            "allowedValuesQuery": "SELECT denial_code FROM insurance_denial_codes",
            "dataType": "string",
            "displayName": "Sec. Denial Code",
            "filterable": true,
            "groupable": true,
