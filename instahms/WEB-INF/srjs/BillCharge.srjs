{
    "allowNoDate": false,
    "dateFields": [
        "finalized_date",
        "posted_date",
        "open_date",
        "closed_date"
    ],
    "defaultOrder": "bill_no",
    "defaultShowFields": [
        "bill_no",
        "bill_patient_full_name",
        "posted_date",
        "chargehead_name",
        "act_description",
        "amount",
        "discount",
        "net_amount"
    ],
    "description": "This report shows all charges added to Bills within the given date range\n  (open, finalized, closed or posted date), the value of the charge and various attributes of the charge.\n  This is similar to the Revenue Report, except that ALL charges are considered, including charges\n  posted to bills that are still in open state.",
    "fields": {
        "visit_id_unique": {
            "table": "b",
            "dataType": "string",
            "displayName": "Unique Visits",
            "description": "Same as Visit ID, but can be used in summary report to give count of unique visits",
            "filterable": false,
            "groupable": false,
            "width": 50,
            "expression": "b.visit_id",
            "aggFunction": "count distinct"
        },
        "primary_sponsor_type": {
            "displayName": "Primary Sponsor Type ",
            "width": "100",
            "dataType": "string",
            "expression": "(pst.sponsor_type_name)",
            "table": "pst",
            "groupable": true,
            "filterable": true
        },
        "secondary_sponsor_type": {
            "displayName": "Secondary Sponsor Type ",
            "width": "100",
            "dataType": "string",
            "expression": "(sst.sponsor_type_name)",
            "table": "sst",
            "groupable": true,
            "filterable": true
        },
        "cond_doc_custom5": {
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "string",
            "displayName": "Cond Custom5",
            "expression": "cdoc.custom_field5_value ",
            "filterable": true,
            "groupable": true,
            "name": "cond_doc_custom5",
            "table": "cdoc",
            "width": 60
        },
        "patient_amount": {
            "aggFunction": "sum",
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "displayName": "Patient Amt w/o Taxes",
            "expression": "CASE WHEN b.is_tpa THEN (COALESCE(bc.amount,0)-COALESCE(bc.insurance_claim_amount,0))::NUMERIC ELSE bc.amount END ",
            "filterable": true,
            "groupable": false,
            "name": "patient_amount",
            "table": "bc",
            "width": 70
        },
        "pri_claim_id": {
            "allowHorizGroup": true,
            "allowNull": false,
            "displayName": "Pri. Claim Id",
            "expression": "pbcl.claim_id",
            "filterable": true,
            "groupable": true,
            "dataType": "string",
            "table": "pbcl",
            "width": 70
        },
        "sec_claim_id": {
            "allowHorizGroup": true,
            "allowNull": false,
            "displayName": "Sec. Claim Id",
            "expression": "sbcl.claim_id",
            "filterable": true,
            "groupable": true,
            "dataType": "string",
            "table": "sbcl",
            "width": 70
        },
        "redeemed_points": {
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "integer",
            "aggFunction": "sum",
            "displayName": "Points Redeemed",
            "filterable": true,
            "groupable": false,
            "name": "redeemed_points",
            "width": 110
        },
        "dr_discount_amt": {
            "aggFunction": "sum",
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "amount",
            "displayName": "Doctor Discount Amt",
            "filterable": true,
            "groupable": false,
            "name": "dr_discount_amt",
            "table": "bc",
            "width": 60
        },
        "posted_time": {
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "string",
            "displayName": "Posted Time",
            "expression": " to_char(posted_date,'HH24:mi')::text  ",
            "filterable": true,
            "groupable": true,
            "name": "posted_time",
            "table": "bc",
            "width": 50
        },
        "claim_recd_total": {
            "aggFunction": "sum",
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "amount",
            "displayName": "Total Sponsor Paid Amt",
            "filterable": true,
            "groupable": false,
            "name": "bc.claim_recd_total",
            "expression": "bc.claim_recd_total",
            "table": "bc",
            "width": 70
        },
        "diff": {
            "aggFunction": "sum",
            "allowHorizGroup": true,
            "allowNull": false,
            "dataType": "numeric",
            "decimalType": "amount",
            "description": "Difference between the billed amount and original amount as per rate plan",
            "displayName": "Amt. Variation",
            "expression": "(act_rate-orig_rate)*act_quantity ",
            "filterable": true,
            "groupable": false,
            "name": "diff",
            "table": "bc",
            "width": 60
        },
        "conducting_doctor": {
            "allowHorizGroup": true,
            "allowNull": true,
            "allowedValuesQuery": "SELECT doctor_name FROM doctors ORDER BY doctor_name",
            "dataType": "string",
            "displayName": "Conducting Doctor",
            "expression": "cdoc.doctor_name ",
            "filterable": true,
            "groupable": true,
            "name": "conducting_doctor",
            "table": "cdoc",
            "width": 110
        },
        "charge_prior_auth_mode_name": {
            "allowHorizGroup": true,
            "allowNull": true,
            "allowedValuesQuery": "SELECT prior_auth_mode_name FROM prior_auth_modes ORDER BY prior_auth_mode_name",
