    },
    "primary_tpa_name": {
      "displayName": "General/Primary Sponsor",
      "width": "120",
      "groupable": true,
      "filterable": true,
      "allowNull": true,
      "allowedValuesQuery": "SELECT 'GENERAL' AS tpa_name UNION SELECT tpa_name FROM tpa_master ORDER BY tpa_name"
