{
  "title": "Current Bed Occupancy Report",
  "reportGroup": "Bed Util Reports",
  "allowNoDate": true,
  "dateFields": ["admission_date","first_visit_date"],
  "description": "This report describes bed occupancy in detail.",
  "tableName": ["rpt_bed_occupancy_report_view"],
  "defaultShowFields": [
    "bed_name",
    "patient_name",
    "age",
    "patient_gender",
    "mr_no",
    "patient_id",
    "doctor_name",
    "admission_datetime",
    "primary_tpa_name",
    "mlc_status",
    "deposit",
    "approval_amount",
    "occupied_status"
  ],
  "fields": {
    "bed_name": {
      "displayName": "Bed Name",
      "width": "100",
      "groupable": true,
      "filterable": true,
      "allowNull": true,
      "allowedValuesQuery": "SELECT 'Not Allocated' AS bed_name UNION SELECT * FROM (SELECT bed_name FROM bed_names GROUP BY bed_name ORDER BY bed_name) AS foo"
    },
    "bed_status": {
      "displayName": "Bed Status",
      "width": "70",
      "groupable": true,
      "filterable": true,
      "allowNull": false,
      "allowedValues": ["Occupied","Vacant"]
    },
    "bed_type": {
      "displayName": "Bed Type",
      "width": "100",
      "groupable": true,
      "filterable": true,
      "allowedValuesQuery": "SELECT bed_type FROM bed_details GROUP BY bed_type ORDER BY bed_type"
    },
    "ward_name": {
      "displayName": "Ward",
      "width": "100",
      "groupable": true,
      "filterable": true,
      "allowedValuesQuery": "SELECT ward_name FROM ward_names GROUP BY ward_name ORDER BY ward_name"
    },
    "patient_name": {
      "displayName": "Patient Name",
      "width": "130"
    },
    "name_local_language": {
             "displayName": "Name In Local Language",
             "width": "150"
         },
    "age": {
      "displayName": "Age",
      "width": "40",
      "dataType": "numeric",
      "decimalType": "integer",
      "groupable": true,
      "filterable": true,
      "allowNull": true,
      "allowedValuesQuery": "SELECT get_patient_age(pd.dateofbirth, pd.expected_dob) AS age FROM patient_details pd GROUP BY age ORDER BY age"
    },
    "patient_gender": {
      "displayName": "Gender",
      "width": "50",
      "groupable": true,
      "filterable": true,
      "allowedValues": ["Male","Female","Couple","Others"]
    },
    "mr_no": {
      "displayName": "MR No.",
      "width": "65"
    },
    "patient_id": {
      "displayName": "Patient ID",
      "width": "65"
    },
    "first_visit_date": {
      "displayName": "First Visit Date",
      "dataType": "date",
      "width": "60"
    },
    "doctor_name": {
      "displayName": "Doctor",
      "width": "120",
      "groupable": true,
      "filterable": true,
      "allowNull": true,
      "allowedValuesQuery": "SELECT doctor_name FROM doctors GROUP BY doctor_name ORDER BY doctor_name"
    },
    "admission_date": {
      "displayName": "Admission Date",
      "dataType": "date",
      "width": "60"
    },
    "admission_time": {
      "displayName": "Admission Time",
      "width": "60"
