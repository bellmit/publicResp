{
	"title": "Complaints Report",
	"reportGroup": "Misc. Report",
	"tableName": [
        "rpt_complaint_view"
    ],
  	"description":"This report shows Complaints on the Asset",

	"dateFields": [
		"raised_date",
		"resolved_date",
		"assigned_date",
		"closed_date"
	],
	"allowNoDate": true,
	"defaultOrder": "complaint_id",
	"defaultShowFields": [
    	"complaint_id",
    	"asset_name",
    	"serial_no",
    	"complaint_type",
    	"complaint_status",
    	"raised_date",
    	"dept_name"
  	],
  	"fields": {
  		"complaint_id" : {
  			"displayName": "Complaint ID",
  			"dataType": "numeric",
  			"filterable": true, "width":"60"
  		},
  		"asset_name" : {
  			"displayName": "Asset Name",
  			"filterable": true, "groupable": true,
  			"width": "120"
  		},
  		"serial_no" : {
  			"displayName": "Serial #",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"complaint_type" : {
  			"displayName": "Complaint Type",
  			"filterable": true, "groupable": true,
  			"width": "80"
  		},
  		"complainant" : {
  			"displayName": "Complainant",
  			"filterable": true, "groupable": true,
  			"width": "80"
  		},
  		"complaint_status" : {
  			"displayName": "Complaint Status",
  			"filterable": true, "groupable": true,
  			"allowedValues": ["Recorded","Assigned","Resolved","Closed"],
  			"width": "80"
  		},
  		"raised_date" : {
  			"displayName": "Raised On ",
  			"dataType": "date",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"resolved_date" : {
  			"displayName": "Resolved On ",
  			"dataType": "date",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"assigned_date" : {
  			"displayName": "Assigned On ",
  			"dataType": "date",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"closed_date" : {
  			"displayName": "Closed On ",
  			"dataType": "date",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"dept_name" : {
  			"displayName" : "Store",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT dept_name FROM stores ORDER BY dept_name",
  			"width": "120"
  		},
  		"center_name" : {
  			"displayName" : "Center",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT center_name FROM hospital_center_master ORDER BY center_name",
  			 "width": "100"
  		},
  		"complaint_desc" : {
  			"displayName" : "Complaint Description",
  			"width": "120"
  		},
  		"complaint_closure_note": {
  			"displayName" : "Closure Note",
  			"width": "100"
  		}
  	}
}