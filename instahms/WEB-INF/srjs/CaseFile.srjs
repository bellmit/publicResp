{
    "title": "Case File Report",
    "reportGroup": "MRD Reports",
    "allowNoDate": true,
    "dateFields": [
        "transaction_date",
        "issued_on",
        "returned_on",
        "indented_date"
    ],
    "description": "This report shows the current status of Case files",
    "tableName": [
        "rpt_case_file_view"
    ],
    "defaultShowFields": [
        "mr_no",
        "casefile_no",
        "patient_name",
        "age",
        "file_status",
        "issued_on",
        "returned_on",
        "indented_date",
        "indented_dept",
        "transaction_date"
    ],
    "fields": {
        "mr_no": {
            "displayName": "MR No.",
            "width": "65"
        },
        "casefile_no": {
            "displayName": "Case File No",
            "width": "65"
        },
        "patient_name": {
            "displayName": "Patient Name",
            "width": "150"
        },
        "name_local_language": {
            "displayName": "Name In Local Language",
            "width": "150"
         },
        "age": {
            "displayName": "Age",
            "width": "30"
        },
        "visit_type": {
            "displayName": "Visit Type",
            "width": "50",
            "groupable": true,
            "filterable": true,
            "allowedValues": [
                "IP",
                "OP"
            ]
        },
        "op_type_name": {
            "displayName": "Visit/OP Type",
            "width": "80",
            "groupable": true,
            "filterable": true,
            "allowedValuesQuery": "SELECT op_type_name FROM op_type_names"
        },
        "patient_gender": {
            "displayName": "Gender",
            "width": "50",
            "groupable": true,
            "filterable": true,
            "allowedValues": [
                "Male",
                "Female",
                "Couple",
                "Others"
            ]
        },
        "case_status": {
            "displayName": "Case Status",
            "width": "50",
            "groupable": true,
            "filterable": true,
            "allowedValues": [
                "Active",
                "Inactive",
                "Pending"
            ]
        },
        "file_status": {
            "displayName": "File Status",
            "width": "50",
            "groupable": true,
            "filterable": true,
            "allowedValues": [
                "Available with MRD",
                "Indented",
                "Issued",
                "Returned",
                "Lost"
            ]
        },
        "issued_to": {
            "displayName": "Issued To",
            "width": "200",
            "groupable": true,
            "filterable": true,
            "allowedValuesQuery": "select file_user_name as issued_to from mrd_casefile_users order by issued_to"
        },
        "created_date": {
            "displayName": "Created On",
            "width": "60",
            "groupable": true,
            "filterable": true,
            "datatype": "date"
        },
        "issued_on": {
            "displayName": "Issued Date",
            "dataType": "date",
            "width": "60",
            "filterable": true
        },
        "issued_time": {
            "displayName": "Issued Time",
            "width": "60"
        },
        "returned_on": {
            "displayName": "Returned Date",
            "dataType": "date",
            "width": "60",
            "filterable": true
        },
        "returned_time": {
            "displayName": "Returned Time",
            "width": "60"
        },
        "indented_date": {
            "displayName": "Indented Date",
            "width": "60",
            "dataType": "date",
            "filterable": true
        },
        "indented_time": {
            "displayName": "Indented Time",
            "width": "60"
        },
        "indented_dept": {
            "displayName": "Indented By",
            "width": "200",
            "filterable": true,
            "groupable": true,
            "allowedValuesQuery": "select dept_name from department order by dept_name"
        },
        "transaction_date": {
            "displayName": "Transaction Date",
            "width": "60",
            "dataType": "date",
            "groupable": true,
            "filterable": true
        },
        "visit_center_name": {
            "displayName": "Visit Center",
            "width": "60",
            "groupable": true,
            "filterable": true,
            "allowedValuesQuery": "select center_name from hospital_center_master order by center_name"
        },
        "revisit_flag": {
            "displayName": "First Visit / Revisit",
            "width": "60",
            "groupable": true,
            "filterable": true,
            "allowedValues": [
                "First Visit",
                "Revisit"
            ]
