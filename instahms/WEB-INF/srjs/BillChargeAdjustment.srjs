{
    "allowNoDate": false,
    "dateFields": [
        "finalized_date",
        "posted_date",
        "open_date",
        "closed_date",
        "mod_time"
    ],
    "defaultOrder": "bill_charge_adjustment_id",
    "defaultShowFields": [
        "bill_no",
        "bill_patient_full_name",
        "posted_date",
        "chargehead_name",
        "act_description",
        "amount",
        "discount",
        "net_amount"
    ],
    "description": "This report shows all bill charge adjustments within the given date range\n  (open, finalized, closed or posted date).",
