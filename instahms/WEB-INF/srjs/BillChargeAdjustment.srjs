    "queryUnits": [
        {
            "joinTables": [
                {
                    "alias": "b",
                    "dependsOn": "bc",
                    "expression": "ON (bc.bill_no = b.bill_no)",
                    "name": "bill",
                    "type": "JOIN"
                },
                {
                    "type": "LEFT",
                    "name": "bill_charge_claim_details_view",
                    "alias": "bccdv",
                    "dependsOn": "bc",
                    "expression": "ON (bccdv.charge_id = bc.charge_id)"
                },
                {
                    "alias": "bcc",
                    "dependsOn": "bc",
                    "expression": "ON (bc.charge_id = bcc.charge_id)",
                    "name": "(SELECT charge_id,sum(insurance_claim_amt) as total_insurance_claim_amt,COALESCE(sum(claim_recd_total),0) as total_claim_recd_total FROM bill_charge_claim GROUP BY charge_id)",
                    "type": "LEFT"
                },
                {
                    "alias": "chc",
                    "dependsOn": "bc",
                    "expression": "ON (chc.chargehead_id = bc.charge_head)",
                    "name": "chargehead_constants",
                    "type": "JOIN"
                },
                {
                    "alias": "cgc",
                    "dependsOn": "chc",
                    "expression": "ON (cgc.chargegroup_id = chc.chargegroup_id)",
                    "name": "chargegroup_constants",
                    "type": "JOIN"
                },
                {
                    "name": "tpa_master",
                    "alias": "ptpa",
                    "dependsOn": "bc",
                    "expression": "ON ptpa.tpa_id = bc.primary_sponsor_id"
                },
                {
                    "name": "sponsor_type",
                    "alias": "pst",
                    "dependsOn": "ptpa",
                    "expression": "ON ptpa.sponsor_type_id = pst.sponsor_type_id"
                },
                {
                    "name": "tpa_master",
                    "alias": "stpa",
                    "dependsOn": "bc",
                    "expression": "ON stpa.tpa_id = bc.secondary_sponsor_id"
                },
                {
                    "name": "sponsor_type",
                    "alias": "sst",
                    "dependsOn": "stpa",
                    "expression": "ON stpa.sponsor_type_id = sst.sponsor_type_id"
                },
                {
                    "alias": "ct",
                    "dependsOn": "bc",
                    "expression": "on (bc.consultation_type_id=ct.consultation_type_id )",
                    "name": "consultation_types",
                    "type": "LEFT"
                },
                {
                    "name": "patient_category_master",
                    "alias": "prcm",
                    "dependsOn": "bc",
                    "expression": "on prcm.category_id = bc.patient_category_id"
                },
                {
                    "alias": "ssg",
                    "dependsOn": "bc",
                    "expression": "ON (ssg.service_sub_group_id = bc.service_sub_group_id)",
                    "name": "service_sub_groups",
                    "type": "LEFT"
                },
                {
                    "alias": "sg",
                    "dependsOn": "bc,ssg",
                    "expression": "ON (sg.service_group_id = ssg.service_group_id)",
                    "name": "service_groups",
                    "type": "LEFT"
                },
                {
                    "alias": "bahc",
                    "dependsOn": "chc",
                    "expression": "ON (bahc.account_head_id = chc.account_head_id)",
                    "name": "bill_account_heads",
                    "type": "JOIN"
                },
                {
                    "alias": "sbah",
                    "dependsOn": "ssg",
                    "expression": "ON (sbah.account_head_id = ssg.account_head_id)",
                    "name": "bill_account_heads"
                },
                {
                    "alias": "bcagm",
                    "dependsOn": "bc",
                    "expression": "ON (bcagm.account_group_id = bc.account_group)",
                    "name": "account_group_master",
                    "type": "LEFT"
                },
                {
                    "alias": "tdep",
                    "dependsOn": "bc",
                    "expression": "ON (tdep.dept_id = bc.act_department_id)",
                    "name": "treating_departments_view",
                    "type": "LEFT"
                },
                {
                    "alias": "cdoc",
                    "dependsOn": "bc",
                    "expression": "ON(cdoc.doctor_id=bc.payee_doctor_id)",
                    "name": "doctors",
                    "type": "LEFT"
                },
                {
                    "alias": "predoc",
                    "dependsOn": "bc",
                    "expression": "ON(predoc.doctor_id=bc.prescribing_dr_id)",
                    "name": "doctors",
                    "type": "LEFT"
                },
                {
                    "alias": "dac",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_dr = dac.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "dap",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_pres_dr = dap.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "dar",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_ref = dar.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "dah",
                    "dependsOn": "bc",
                    "expression": "ON (bc.discount_auth_hosp = dah.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "daov",
                    "dependsOn": "bc",
                    "expression": "ON (bc.overall_discount_auth = daov.disc_auth_id)",
                    "name": "discount_authorizer",
                    "type": "LEFT"
                },
                {
                    "alias": "pamc",
                    "dependsOn": "bc",
                    "expression": "ON (pamc.prior_auth_mode_id = bc.prior_auth_mode_id)",
                    "name": "prior_auth_modes",
                    "type": "LEFT"
                }
            ],
            "mainTableAlias": "bc",
            "mainTableName": "bill_charge_adjustment_unsorted_view"
        }
    ],
    "reportGroup": "Billing Reports",
    "title": "Bill Charge Adjustment Report"
}