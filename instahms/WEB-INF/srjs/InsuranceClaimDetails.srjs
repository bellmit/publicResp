                },
                "patient_amt_wo_tax": {
                    "table": "s,pscl,sscl",
                    "expression": "(COALESCE(s.amount,0) - COALESCE(pscl.insurance_claim_amt,0) - COALESCE(sscl.insurance_claim_amt,0)  - COALESCE(s.tax,0) )"
                },
                "tax_amt": {
                    "table": "s",
