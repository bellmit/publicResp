{
    "title": "Insurance Claim Details Report",
    "reportGroup": "Insurance Reports",
    "dateFields": [
        "pri_created_date",
        "pri_submission_date",
        "open_date",
        "finalized_date",
        "closed_date",
        "posted_date"
    ],
    "allowNoDate": true,
    "defaultOrder": "bill_no",
    "description": "This report shows all charges added to the Bills connected to Insurance, within the specified date range.",
    "includes": [
        "PatientDetailFields.srjs",
        "PatientVisitFields.srjs",
        "BillDetailFields.srjs"
    ],
    "queryUnits": [
        {
            "mainTableName": "bill_charge",
            "mainTableAlias": "bcc",
            "joinTables": [
                {
                    "type": "JOIN",
                    "name": "chargehead_constants",
                    "alias": "chc",
                    "dependsOn": "bcc",
                    "expression": "ON (chc.chargehead_id = bcc.charge_head)"
                },
                {
                    "type": "JOIN",
                    "name": "chargegroup_constants",
                    "alias": "cgc",
                    "dependsOn": "chc",
                    "expression": "ON (cgc.chargegroup_id = chc.chargegroup_id)"
                },
                {
                    "name": "consultation_types",
                    "alias": "ct",
                    "dependsOn": "bcc",
                    "expression": "on (bcc.consultation_type_id=ct.consultation_type_id )"
                },
                {
                    "name": "service_sub_groups",
                    "alias": "ssg",
                    "dependsOn": "bcc",
                    "expression": "ON (ssg.service_sub_group_id = bcc.service_sub_group_id)"
                },
                {
                    "name": "service_groups",
                    "alias": "sg",
                    "dependsOn": "ssg",
                    "expression": "ON (sg.service_group_id = ssg.service_group_id)"
                },
                {
                    "type": "JOIN",
                    "name": "bill_account_heads",
                    "alias": "bahc",
                    "dependsOn": "chc",
                    "expression": "ON (bahc.account_head_id = chc.account_head_id)"
                },
                {
                    "name": "account_group_master",
                    "alias": "bcagm",
                    "dependsOn": "bcc",
                    "expression": "ON (bcagm.account_group_id = bcc.account_group)"
                },
                {
                    "name": "treating_departments_view",
                    "alias": "tdep",
                    "dependsOn": "bcc",
                    "expression": "ON (tdep.dept_id = bcc.act_department_id)"
                },
                {
                    "name": "doctors",
                    "alias": "cdoc",
                    "dependsOn": "bcc",
                    "expression": "ON(cdoc.doctor_id=bcc.payee_doctor_id)"
                },
                {
                    "name": "doctors",
                    "alias": "predoc",
                    "dependsOn": "bcc",
                    "expression": "ON (predoc.doctor_id=bcc.prescribing_dr_id)"
                },
                {
                    "name": "discount_authorizer",
                    "alias": "daov",
                    "dependsOn": "bcc",
                    "expression": "ON (bcc.overall_discount_auth = daov.disc_auth_id)"
                },
                {
                    "type": "JOIN",
                    "name": "bill",
                    "alias": "b",
                    "dependsOn": "bcc",
                    "expression": "ON (bcc.bill_no = b.bill_no AND is_tpa)"
                },
                {
                    "type": "LEFT",
                    "name": "bill_claim",
                    "alias": "pbcl",
                    "dependsOn": "b",
                    "expression": "ON(b.bill_no = pbcl.bill_no AND pbcl.priority = 1)"
                },
                {
                    "type": "LEFT",
                    "name": "bill_charge_claim",
                    "alias": "pbcc",
                    "dependsOn": "pbcl,bcc",
                    "expression": "ON(pbcl.claim_id = pbcc.claim_id AND pbcc.charge_id = bcc.charge_id )"
                },
                {
                    "type": "LEFT",
                    "name": "insurance_claim",
                    "alias": "picl",
                    "dependsOn": "pbcl",
                    "expression": "ON(pbcl.claim_id = picl.claim_id)"
                },
                {
                    "type": "LEFT",
                    "name": "bill_claim",
                    "alias": "sbcl",
                    "dependsOn": "b",
                    "expression": "ON(b.bill_no = sbcl.bill_no AND sbcl.priority =2)"
                },
                {
                    "type": "LEFT",
                    "name": "bill_charge_claim",
                    "alias": "sbcc",
                    "dependsOn": "sbcl,bcc",
                    "expression": "ON(sbcl.claim_id = sbcc.claim_id AND bcc.charge_id = sbcc.charge_id)"
                },
                {
                    "type": "LEFT",
                    "name": "insurance_claim",
                    "alias": "sicl",
                    "dependsOn": "sbcl",
                    "expression": "ON(sbcl.claim_id = sicl.claim_id)"
                },
                {
                    "type": "LEFT",
                    "name": "(SELECT remittance_id, charge_id, claim_id, row_number() OVER (PARTITION BY charge_id, claim_id ORDER by remittance_id DESC) as rank from insurance_payment_allocation)",
                    "alias": "pipa",
                    "dependsOn": "pbcc",
                    "expression": "ON(pbcc.claim_id = pipa.claim_id AND pbcc.charge_id = pipa.charge_id AND pipa.rank = 1)"
                },
                {
                    "type": "LEFT",
                    "name": "insurance_remittance",
                    "alias": "pir",
                    "dependsOn": "pipa",
                    "expression": "ON(pir.remittance_id = pipa.remittance_id)"
                },
                {
                    "type": "LEFT",
                    "name": "(SELECT remittance_id, charge_id, claim_id, row_number() OVER (PARTITION BY charge_id, claim_id ORDER by remittance_id DESC) as rank from insurance_payment_allocation)",
                    "alias": "sipa",
                    "dependsOn": "sbcc",
                    "expression": "ON(sbcc.claim_id = sipa.claim_id AND sbcc.charge_id = sipa.charge_id AND sipa.rank = 1)"
                },
                {
                    "type": "LEFT",
                    "name": "insurance_remittance",
                    "alias": "sir",
                    "dependsOn": "sipa",
                    "expression": "ON(sir.remittance_id = sipa.remittance_id)"
                },
                {
                    "name": "prior_auth_modes",
                    "alias": "pamc",
                    "dependsOn": "pbcc",
                    "expression": "ON (pamc.prior_auth_mode_id = pbcc.prior_auth_mode_id)"
                },
                {
                    "name": "prior_auth_modes",
                    "alias": "samc",
                    "dependsOn": "sbcc",
                    "expression": "ON (samc.prior_auth_mode_id = sbcc.prior_auth_mode_id)"
                },
                {
                    "includeName": "BillDetailFields.srjs"
                },
                {
                    "includeName": "PatientVisitFields.srjs"
                },
                {
                    "includeName": "PatientDetailFields.srjs"
                },
                {
                    "name": "item_insurance_categories",
                    "alias": "it_c",
                    "dependsOn": "bcc",
                    "expression": "ON (it_c.insurance_category_id = bcc.insurance_category_id)"
                },
                {
                    "name": "insurance_denial_codes",
                    "alias": "pidc",
                    "dependsOn": "pbcc",
                    "expression": "ON (pidc.denial_code = pbcc.denial_code)"
                },
                {
                    "name": "insurance_denial_codes",
                    "alias": "sidc",
                    "dependsOn": "sbcc",
                    "expression": "ON (sidc.denial_code = sbcc.denial_code)"
                },
                {
                    "name": "insurance_submission_batch",
                    "alias": "prifirstsub",
                    "dependsOn": "picl",
                    "expression": "ON (prifirstsub.submission_batch_id = picl.last_submission_batch_id AND prifirstsub.is_resubmission = 'N')"
                },
                {
                    "name": "insurance_submission_batch",
                    "alias": "secfirstsub",
                    "dependsOn": "sicl",
                    "expression": "ON (secfirstsub.submission_batch_id = sicl.last_submission_batch_id AND secfirstsub.is_resubmission = 'N')"
                },
                {
                    "name": "(SELECT claim_id, max(submission_batch_id) as submission_batch_id FROM claim_submissions GROUP by claim_id)",
                    "alias": "pcs",
                    "dependsOn": "picl",
                    "expression": "ON (pcs.claim_id = picl.claim_id)"
                },
                {
                    "name": "insurance_submission_batch",
                    "alias": "priresub",
                    "dependsOn": "pcs",
                    "expression": "ON (priresub.submission_batch_id = pcs.submission_batch_id)"
                },
                {
                    "name": "(SELECT claim_id, max(submission_batch_id) as submission_batch_id FROM claim_submissions GROUP by claim_id)",
                    "alias": "scs",
                    "dependsOn": "sicl",
                    "expression": "ON (scs.claim_id = sicl.claim_id)"
                },
                {
                    "name": "insurance_submission_batch",
                    "alias": "secresub",
                    "dependsOn": "scs",
                    "expression": "ON (secresub.submission_batch_id = scs.submission_batch_id)"
                },
                {
                    "name": "(SELECT count(cs.submission_batch_id) AS resubmission_count, cs.claim_id FROM insurance_claim ic JOIN claim_submissions cs ON (cs.claim_id = ic.claim_id) JOIN insurance_submission_batch isb ON (cs.submission_batch_id = isb.submission_batch_id) WHERE isb.is_resubmission = 'Y' GROUP BY cs.claim_id)",
                    "alias": "pcsx",
                    "dependsOn": "picl",
                    "expression": "ON (picl.claim_id = pcsx.claim_id)"
                },
                {
                    "name": "(SELECT count(cs.submission_batch_id) AS resubmission_count, cs.claim_id FROM insurance_claim ic JOIN claim_submissions cs ON (cs.claim_id = ic.claim_id) JOIN insurance_submission_batch isb ON (cs.submission_batch_id = isb.submission_batch_id) WHERE isb.is_resubmission = 'Y' GROUP BY cs.claim_id)",
                    "alias": "scsx",
                    "dependsOn": "sicl",
                    "expression": "ON (sicl.claim_id = scsx.claim_id)"
                },
                {
                    "name": "mrd_diagnosis",
                    "alias": "pdiag",
                    "dependsOn": "pr",
                    "expression": "ON (pdiag.diag_type = 'P' AND pdiag.visit_id = pr.patient_id)"
                },
                {
                    "name": "mrd_diagnosis",
                    "alias": "vdiag",
                    "dependsOn": "pr",
                    "expression": "ON (vdiag.diag_type = 'V' AND vdiag.visit_id = pr.patient_id)"
                },
                {
                    "type": "LEFTLATERAL",
                    "name": "(SELECT mrdd.visit_id,textcat_linecat(mrdd.description::text) AS secondary_diagnosis,textcat_linecat(mrdd.icd_code::text) AS secondary_diag_icd_code,textcat_linecat(mrdd.code_type::text) AS secondary_code_type FROM mrd_diagnosis mrdd WHERE mrdd.visit_id = pr.patient_id AND mrdd.diag_type = 'S' GROUP BY mrdd.visit_id)",
                    "alias": "sdiag",
                    "dependsOn": "pr",
                    "expression": "ON (true)"
                },
                {
                    "name": "mrd_supported_code_types",
                    "alias": "msct",
                    "dependsOn": "bcc",
                    "expression": "ON (msct.code_type = bcc.code_type)"
                },
                {
                    "type": "LEFT",
                    "name": "doctors",
                    "alias": "aDoc",
                    "dependsOn": "pr",
                    "expression": "ON(aDoc.doctor_id = pr.doctor)"
                },
                {
                    "type": "LEFT",
                    "name": "(SELECT 'Referal' AS doctor_type,referal_no, referal_name, clinician_id AS doctor_license_number FROM referral UNION ALL SELECT 'Doctor' AS doctor_type,doctor_id, doctor_name, doctor_license_number FROM doctors)",
                    "alias": "refDoc",
                    "dependsOn": "pr",
                    "expression": "ON(refDoc.referal_no = pr.reference_docto_id)"
                }
            ],
            "whereExpression": " (b.status != 'X') AND bcc.status!='X' AND bcc.charge_head NOT IN ('PHMED','PHCMED','PHRET','PHCRET') ",
            "whereTable": "b"
        },
