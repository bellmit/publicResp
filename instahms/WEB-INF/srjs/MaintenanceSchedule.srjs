{
	"title": "Maintenance Schedule Report",
	"reportGroup": "Misc. Report",
	"tableName": [
        "rpt_maintenance_schedule"
    ],
  	"description":"This report gives the details of Maintenance Schedule on Assets",

  	"dateFields": [
		"installation_date",
		"next_maint_date"
	],
	"allowNoDate": true,
	"defaultOrder": "maint_id",
	"defaultShowFields": [
    	"maint_id",
    	"asset_name",
    	"serial_no",
    	"maint_frequency",
    	"next_maint_date",
    	"contractor_name",
    	"asset_category"
  	],
  	"fields": {
		"maint_id" : {
  			"displayName": "Maint ID",
  			"filterable": true, "width":"60",
  			"dataType": "numeric"
  		},
  		"asset_name" : {
  			"displayName": "Asset Name",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT medicine_name FROM fixed_asset_master a JOIN store_item_details m ON (m.medicine_id = a.asset_id)",
  			"width": "120"
  		},
  		"serial_no" : {
  			"displayName": "Serial #",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"maint_frequency" : {
  			"displayName": "Frequency",
  			"filterable": true, "groupable":true,
  			"allowedValues": ["Weekly","Every two weeks","Monthly","Quarterly","Semi-annually", "Annually", "Custom"],
  			"width": "80"
  		},
  		"next_maint_date" : {
  			"displayName": "Next Maint Date",
  			"filterable": true, "groupable": true,
  			"dataType": "date",
  			"width": "60"
  		},
  		"contractor_name": {
  			"displayName": "Contractor",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT contractor_name FROM contractor_master ORDER BY contractor_name",
  			"width": "60"
  		},
  		"asset_category" : {
  			"displayName": "Asset Category",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT category FROM store_category_master",
  			"width": "80"
  		},
  		"asset_id" : {
  			"displayName": "Asset ID",
  			"filterable": true, "width":"60",
  			"dataType": "numeric", "decimalType": "integer"
  		},
  		"department" : {
  			"displayName": "Hospital Dept",
  			"width": "100"
  		},
  		"department_contact" : {
  			"displayName": "Hospital Dept Contact",
  			"width": "80"
  		},
  		"installation_date": {
  			"displayName": "Installation Date",
  			"dataType": "date",
  			"filterable": true, "groupable": true,
  			"width": "60"
  		},
  		"asset_purchase_val": {
  			"displayName": "Purchase Value",
  			"width": "40"
  		},
  		"asset_make": {
  			"displayName": "Asset Make",
  			"width": "70"
  		},
  		"asset_model": {
  			"displayName": "Asset Model",
  			"width": "80"
  		},
  		"dept_name" : {
  			"displayName" : "Store",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT dept_name FROM stores ORDER BY dept_name",
  			"width": "120"
  		},
  		"center_name" : {
  			"displayName" : "Center",
  			"filterable": true, "groupable": true,
  			"allowedValuesQuery": "SELECT center_name FROM hospital_center_master ORDER BY center_name",
  			 "width": "100"
  		}
  	}
 }
