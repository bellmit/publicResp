	<queryString>
		<![CDATA[SELECT *,  (age||agein) || '/' || patient_gender as patient_age, hospital_advance+pharmacy_advance as advance,
total_amt - (hospital_advance+pharmacy_advance) as due
FROM (
	SELECT  pr.mr_no, pr.status,  wn.ward_name, pr.bed_type, 
		bn.bed_name, 
		get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as patient_name,
		b.total_amount - b.total_claim AS total_amt,
		(SELECT CASE WHEN pr.primary_sponsor_id IS NOT NULL and pr.primary_sponsor_id != '' 
				THEN coalesce(sum(amount-insurance_claim_amount),0.00)
				ELSE coalesce(sum(amount),0.00) END
			FROM bill_charge bc
			WHERE bc.bill_no=b.bill_no AND charge_head IN ('PHMED','PHRET','PHCMED','PHCRET')
				AND bc.status !='X' 
			) AS pharmacy_amt,
		(SELECT coalesce(sum(brh.amount), 0.00)
			FROM bill_receipts brh
				JOIN counters ch ON (brh.counter = ch.counter_id)
			WHERE brh.bill_no = b.bill_no AND brh.payment_type IN ('R','F') AND ch.counter_type != 'P'
			) AS hospital_advance, 
		(SELECT coalesce(sum(brp.amount), 0.00) 
			FROM bill_receipts brp
				JOIN counters cp ON (brp.counter = cp.counter_id)
			WHERE brp.bill_no = b.bill_no AND brp.payment_type IN ('R','F') AND cp.counter_type = 'P'
			) AS pharmacy_advance,
			get_patient_age(pd.dateofbirth, pd.expected_dob) AS age,
			get_patient_age_in(pd.dateofbirth, pd.expected_dob) AS agein,
			pd.patient_gender,
			a.admit_date,
			pr.discharge_date,
			d.doctor_name as admitting_doctor,
			od.org_name, 
			tm.tpa_name,
			om.operation_name,
			date(bos.start_datetime) as date_of_surgery
			
	FROM bill b
		LEFT JOIN patient_registration pr ON (pr.patient_id=b.visit_id)
		LEFT JOIN patient_details pd on (pd.mr_no=pr.mr_no)
		LEFT JOIN admission a ON (a.patient_id=pr.patient_id)
		LEFT JOIN bed_names bn ON (bn.bed_id=a.bed_id)
		LEFT JOIN ward_names wn ON (wn.ward_no=bn.ward_no)
		LEFT JOIN doctors d ON  (d.doctor_id=pr.doctor)
		LEFT JOIN organization_details od ON ( od.org_id = pr.org_id )
		LEFT JOIN tpa_master tm ON ( tm.tpa_id = pr.primary_sponsor_id )
		LEFT JOIN bed_operation_schedule bos ON (bos.mr_no = pr.mr_no)
		LEFT JOIN operation_master om ON (om.op_id = bos.operation_name)
		WHERE b.status IN ('A','F') AND bill_type IN ('P','C') AND b.visit_type='i'
		 
) as foo where status='A' and ward_name is not null 
group by bed_name,mr_no,status,ward_name,bed_type,patient_name,
total_amt,pharmacy_amt,hospital_advance,pharmacy_advance,age,agein,patient_gender,
admit_date,discharge_date,admitting_doctor,org_name, tpa_name,operation_name,date_of_surgery
 order by ward_name]]>
	</queryString>
