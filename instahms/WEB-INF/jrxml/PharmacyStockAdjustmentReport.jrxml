	<parameter name="adjNo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT distinct s.DEPT_NAME,medicine_name,sibd.batch_no,
CASE WHEN type = 'A' THEN sa.qty ELSE -sa.qty END as qty,sam.username,reason,adj_no,to_char(date_time,'DD-MM-YYYY') as date,
case when type = 'A' then 'Increase'  else 'Decrease' end as adjtype,
sa.cost_value AS amt
FROM store_adj_details sa
JOIN store_adj_main sam using (adj_no)
JOIN store_item_details sitd using(medicine_id)
JOIN stores s on DEPT_ID=store_id
JOIN store_item_batch_details sibd ON(sibd.item_batch_id = sa.item_batch_id)
WHERE sa.item_batch_id = sibd.item_batch_id
and adj_no='$P!{adjNo}']]>
