	<queryString>
		<![CDATA[SELECT PD.MR_NO,SM.SALUTATION,PD.PATIENT_NAME,PD.LAST_NAME,PD.PATIENT_GENDER,
CASE WHEN PD.PATIENT_GENDER='M' THEN 'MALE' ELSE 'FEMALE' END AS GENDER,
get_patient_age(pd.dateofbirth, pd.expected_dob) as patient_age,
get_patient_age_in(pd.dateofbirth, pd.expected_dob) as patient_age_in,
PD.PATIENT_PHONE,PD.PATIENT_ADDRESS,C.CITY_NAME,STM.STATE_NAME,
PD.OLDMRNO,PRA.PATIENT_ID,PRA.REG_DATE,PRA.REG_TIME,PRA.DOCTOR,DR.DOCTOR_NAME,PRA.DEPT_NAME AS DEPT_ID,
DEP.DEPT_NAME,EH.ORGANIZATION_ID,OD.ORG_NAME,EH.BED_TYPE,IC.INSURANCE_ID
FROM
insurance_case ic join tpa_master tpa on tpa.tpa_id=ic.tpa_id
join insurance_estimate ie on ie.insurance_id = ic.insurance_id
join estimate_header eh on eh.estimate_id=ie.estimate_id
join organization_details od on od.org_id=eh.organization_id
join patient_details pd on pd.mr_no=ic.mr_no
LEFT JOIN PATIENT_REGISTRATION PRA ON (PD.MR_NO = PRA.MR_NO AND PRA.STATUS='A')
LEFT JOIN SALUTATION_MASTER SM ON (PD.SALUTATION = SM.SALUTATION_ID)
LEFT JOIN CITY C ON C.CITY_ID=PD.PATIENT_CITY
LEFT JOIN STATE_MASTER STM ON  STM.STATE_ID=PD.PATIENT_STATE
LEFT JOIN DEPARTMENT DEP ON (PRA.DEPT_NAME = DEP.DEPT_ID)
LEFT JOIN DOCTORS DR ON (PRA.DOCTOR = DR.DOCTOR_ID)
WHERE IC.INSURANCE_ID=$P{insuranceId}]]>
	</queryString>
