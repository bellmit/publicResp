	<queryString>
		<![CDATA[SELECT mr_no, visit_id, bill_no, b.visit_type, patient_full_name,
total_amount, total_discount, total_claim, coalesce(total_receipts,0) as total_receipts,COALESCE(deposit_set_off,0) as deposits  ,patient_name
FROM bill b  
JOIN patient_visit_details_ext_view pd ON (pd.patient_id = b.visit_id)

WHERE (b.status = 'C' OR b.status = 'S')
AND COALESCE(total_amount,0) > COALESCE(total_receipts,0) + COALESCE(total_claim,0) +COALESCE(deposit_set_off)
AND (date(open_date) between $P{fromDate} AND $P{toDate})  ORDER BY open_date]]>
