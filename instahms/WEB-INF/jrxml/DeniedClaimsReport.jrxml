	<queryString>
		<![CDATA[SELECT ic.mr_no,get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) AS patname,
ic.insurance_id AS case_no, ic.case_added_date, ic.insurance_no,b.total_amount AS bill_amount,
ic.status_reason, tpam.tpa_name
FROM insurance_case ic
LEFT JOIN patient_details pd ON pd.mr_no::text = ic.mr_no::text
LEFT OUTER JOIN patient_registration pr ON pd.visit_id = pr.patient_id
LEFT JOIN bill b ON ic.bill_no::text=b.bill_no::text
LEFT JOIN tpa_master tpam ON ic.tpa_id::text=tpam.tpa_id::text
WHERE ic.status='D'
AND date(ic.case_added_date) between $P{fromDate} and $P{toDate}]]>
