	<parameter name="center" class="java.lang.String"/>
	<queryString>
		<![CDATA[
		SELECT bc.charge_id, b.bill_no, bc.act_description, bc.act_remarks, bc.amount,
			chc.chargehead_name, b.finalized_date, pr.mr_no, bcag.account_group_name,
			COALESCE (get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name),
				prc.customer_name, isr.patient_name) as patient_full_name
		FROM bill b
			JOIN bill_charge bc ON (bc.bill_no=b.bill_no)
			JOIN chargehead_constants chc ON (chc.chargehead_id = bc.charge_head)
			JOIN account_group_master bag ON (bag.account_group_id = b.account_group)
			JOIN account_group_master bcag ON (bcag.account_group_id = bc.account_group)
			LEFT JOIN patient_registration pr ON (pr.patient_id = b.visit_id)
			LEFT JOIN store_retail_customers prc ON (prc.customer_id = b.visit_id)
			LEFT JOIN incoming_sample_registration isr ON (isr.incoming_visit_id = b.visit_id)
			LEFT JOIN patient_details pd ON (pr.mr_no = pd.mr_no)
			LEFT JOIN u_user u ON b.username = u.emp_username
		WHERE bc.account_group != b.account_group AND b.account_group=1
			AND b.finalized_date::date BETWEEN $P{fromDate} AND $P{toDate}
			AND b.status != 'X' AND bc.status != 'X'
			AND ( $P!{center}=0 OR COALESCE(pr.center_id, prc.center_id, isr.center_id) = $P!{center} )
		ORDER BY bcag.account_group_name]]>
	</queryString>
