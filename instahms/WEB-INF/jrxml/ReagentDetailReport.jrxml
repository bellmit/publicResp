	<queryString>
		<![CDATA[select d.test_name as name,itd.medicine_name AS item_name,sum(qty) as qty,conducted_date as cdate,
case when tp.conducted='C' then 'Conducted'
when tp.conducted='N' then 'Not Conducted'
when tp.conducted='P' then 'Partially Conducted'
when tp.conducted='X' then 'Cancelled'
when tp.conducted='Y' then 'SignOff' end  as status
from store_reagent_usage_main rm
join store_reagent_usage_details using(reagent_usage_seq)
join tests_conducted tc on prescribed_id=rm.ref_no and consumer_id=test_id
left join diagnostics d using(test_id)
left join store_item_details itd using (medicine_id)
left join diagnostics_reagents dr on dr.test_id=d.test_id and dr.reagent_id=itd.medicine_id
left join tests_prescribed tp on tp.prescribed_id=rm.ref_no and consumer_id=tp.test_id
join stores s ON (s.dept_id=store_id)
where dept_id = -1
and date(conducted_date) >=$P{fromDate} AND date(conducted_date) <=$P{toDate}
group by medicine_name,test_name,conducted_date,tp.conducted

UNION ALL

select s.service_name as name,itd.medicine_name AS item_name,sum(qty) as qty,conducteddate as cdate,
case when sp.conducted='C' then 'Conducted'
when sp.conducted='N' then 'Not Conducted'
when sp.conducted='P' then 'Partially Conducted'
when sp.conducted='X' then 'Cancelled' end as status
from store_reagent_usage_main rm
join store_reagent_usage_details using(reagent_usage_seq)
join services_prescribed sp on prescription_id=rm.ref_no and consumer_id=service_id
left join services s using(service_id)
left join store_item_details itd using (medicine_id)
join stores ss ON (ss.dept_id=store_id)
where dept_id = -2
and date(conducteddate) >=$P{fromDate} AND date(conducteddate) <=$P{toDate}
group by medicine_name,name,conducteddate,sp.conducted
order by item_name]]>
	</queryString>
