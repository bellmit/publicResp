	<style name="Crosstab Data Text" isDefault="false" hAlign="Center"/>
	<parameter name="fromDate" class="java.util.Date"/>
	<parameter name="toDate" class="java.util.Date"/>
	<parameter name="deptIdArray" class="java.lang.String[]">
