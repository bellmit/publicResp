	<queryString>
		<![CDATA[SELECT * FROM (SELECT count (sp.service_id),s.service_name,
sd.department, sp.service_id, sp.patient_id AS pat_id,
sp.presc_date AS pres_date,
COALESCE(pr.visit_type, 'o') AS patienttype
FROM services_prescribed sp
LEFT JOIN patient_registration pr ON (sp.patient_id = pr.patient_id)
LEFT JOIN incoming_sample_registration isr ON (sp.patient_id = isr.incoming_visit_id)
JOIN services s ON (sp.service_id = s.service_id)
JOIN services_departments sd ON (s.dept_name = sd.department)
WHERE date(sp.presc_date) BETWEEN $P{fromDate} AND $P{toDate}
AND $X{IN, sp.conducted , serviceStatusArray}
AND $X{IN, sd.department, deptIdArray}
GROUP BY s.service_name, sd.department, sp.service_id, sp.patient_id, sp.presc_date,visit_type
ORDER BY sp.patient_id
) AS allservices
where $X{IN, patienttype, patientTypeArray}]]>
	</queryString>
