	</parameter>
	<parameter name="serviceStatusArray" class="java.lang.String[]">
		<defaultValueExpression><![CDATA[new String[]{}]]></defaultValueExpression>
	</parameter>
	<parameter name="patientTypeArray" class="java.lang.String[]">
