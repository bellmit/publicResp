	<parameter name="fromdate" class="java.util.Date" isForPrompting="false"/>
	<parameter name="todate" class="java.util.Date" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT 0 as no,sum(bc.amount) as rev, coalesce (pr.visit_type,b.visit_type) as visit_type,  
case when  coalesce (extract(year from age(coalesce(dateofbirth,expected_dob))),isr.patient_age)<1 then '0-1  year' 
when  coalesce (extract(year from age(coalesce(dateofbirth,expected_dob))),isr.patient_age) between 1 and 12  then '1-12 years' 
when  coalesce (extract(year from age(coalesce(dateofbirth,expected_dob))),isr.patient_age) between 12 and 40 then '12-40 years'   
when  coalesce (extract(year from age(coalesce(dateofbirth,expected_dob))),isr.patient_age) between 40 and 60 then  '40-60  years'
else '>60 years' end as  agegroup,  
case when pd.patient_gender like 'M' then 'Male' when pd.patient_gender like 'F' then 'Female' end as gender  
FROM bill_charge bc 
JOIN bill b on   bc.bill_no=b.bill_no  and b.status!='X' and bc.status!='X' and b.visit_type!='r'  
left JOIN patient_registration pr ON b.visit_id=pr.patient_id 
left JOIN patient_details pd on pd.mr_no=pr.mr_no  
left join incoming_sample_registration isr on isr.incoming_visit_id = b.visit_id
WHERE date_trunc('day',bc.posted_date) >= '$P!{fromdate}' and date_trunc('day',bc.posted_date) <= '$P!{todate}'
group by pr.visit_type,dateofbirth,expected_dob,pd.patient_gender,pr.patient_id,b.visit_type,isr.patient_age
UNION ALL  
SELECT count(pr.patient_id) as no,0 as rev, pr.visit_type, case when extract(year from age(coalesce(dateofbirth,expected_dob)))<1 then '0-1  year'  when  extract(year from age(coalesce(dateofbirth,expected_dob))) between 1 and 12  then '1-12 years' 
when  extract(year from age(coalesce(dateofbirth,expected_dob))) between 12 and 40 then '12-40 years'   
when  extract(year from age(coalesce(dateofbirth,expected_dob))) between 40 and 60 then  '40-60  years'
else '>60 years' end as  agegroup , 
case when pd.patient_gender like 'M' then 'Male'  when pd.patient_gender like 'F' then 'Female' end as gender   
FROM patient_registration pr 
left JOIN patient_details pd on pd.mr_no=pr.mr_no  
WHERE date_trunc('day',pr.reg_date) >= '$P!{fromdate}' and date_trunc('day',pr.reg_date) <= '$P!{todate}'
group by pr.visit_type,dateofbirth,expected_dob,patient_gender ,pr.patient_id

union all

Select count(isr.incoming_visit_id) as no,0 as rev,b.visit_type,case when isr.patient_age <1 then '0-1 year'
when isr.patient_age between 1 and 12 then '1-12 years' when isr.patient_age between 12 and 40 then '12-40 years'
when isr.patient_age between 40 and 60 then '40-60 years' else '>60 years' end as agegroup,
case when isr.patient_gender like 'M' then 'Male' when isr.patient_gender like 'F' then 'Female' end as gender
from incoming_sample_registration  isr
left join bill b on b.bill_no = isr.billno
WHERE date_trunc('day',isr.date) >= '$P!{fromdate}' and date_trunc('day',isr.date) <= '$P!{todate}'
group by b.visit_type,isr.patient_age,isr.patient_gender ]]>
	</queryString>
