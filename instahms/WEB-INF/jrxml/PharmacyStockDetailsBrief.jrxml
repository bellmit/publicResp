	<queryString>
		<![CDATA[SELECT medicine_name,batch_no,category_name,generic_name,control_type_name,exp_dt,issue_base_unit,package_type,manf_name,
qty,reorder_level,dept_name,tax_type,mrp,package_cp, group_name,
SUM(mrp_value)AS mrp_value,SUM(costprice_value)AS costprice_value  FROM
(select medicine_name,batch_no,category_name,generic_name,control_type_name,exp_dt,issue_base_unit,package_type,manf_name,
qty,reorder_level,dept_name,tax_type,mrp,package_cp, $P!{groupBy} as group_name,
ROUND((qty*(mrp/issue_base_unit)),2)AS mrp_value,
(ROUND((qty*(package_cp/issue_base_unit)),2))AS costprice_value from $P!{fromView}
WHERE (($P!{primaryFilterBy}=$P{primaryFilterValue})or ($P{primaryFilterValue}='*')) AND ((checkpoint_name::character varying=$P{checkPointName})or ($P{checkPointName}='*')) AND $P!{secondaryFilterBy} BETWEEN $P{fromSecondaryFilterValInt} AND $P{toSecondaryFilterValInt} AND
date(exp_dt) BETWEEN $P{fromDate} AND $P{toDate}) as ds
GROUP BY dept_name,group_name,medicine_name,batch_no,category_name,generic_name,control_type_name,exp_dt,issue_base_unit,package_type,manf_name,
qty,reorder_level,tax_type,mrp,package_cp,group_name ORDER BY dept_name,group_name]]>
