	<parameter name="supplier_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT s.supplier_name,medicine_name,sr.batch_no,sr.qty as qty,user_name,return_no,to_char(srm.date_time,'DD-MM-YYYY') as date,
case when return_type='D' then 'Damage' when return_type='E' then 'Expiry' when return_type='N' then 'Non-Moving' else 'Others' end as returntype,
round((ssd.package_cp/ssd.stock_pkg_size)*sr.qty,2) as amt,orig_return_no,
case when srm.status='O' then 'Open' when srm.status='C' then 'Closed' when srm.status='P' then 'Partially Received' else 'Received' end as retstatus,
case when orig_return_no is null then 'R' else 'X' end as origstatus
FROM store_supplier_returns sr
JOIN store_supplier_returns_main srm using (return_no)

JOIN supplier_master s on (s.supplier_code = supplier_id)
JOIN store_item_details sitd using(medicine_id)
JOIN store_stock_details ssd on dept_id=srm.store_id and ssd.medicine_id=sr.medicine_id and sr.batch_no=ssd.batch_no
WHERE ($P{supplier_id} = '' OR supplier_id = $P{supplier_id})  and
date_trunc('day', srm.date_time) >= '$P!{fromDate} ' AND date_trunc('day', srm.date_time) <=' $P!{toDate}'
GROUP BY s.supplier_name,medicine_name,sr.batch_no,sr.qty,user_name,srm.status,returntype,return_no,
date,ssd.package_cp,ssd.stock_pkg_size,orig_return_no,ret_qty_unit
ORDER BY s.supplier_name,return_no]]>
