<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="CancelledBillPrint1" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="40" bottomMargin="40">
	<parameter name="fromdate" class="java.util.Date"/>
	<parameter name="todate" class="java.util.Date"/>
	<parameter name="doctorId" class="java.lang.String"/>
	<parameter name="departmentId" class="java.lang.String"/>
	<parameter name="userName" class="java.lang.String"/>
	<queryString>
		<![CDATA[select pr.mr_no, b.bill_no, b.visit_id, b.bill_type,to_date(to_char(b.open_date,'dd-mm-yyyy'),'dd-mm-yyyy') as open_date,coalesce(b.remarks,bc.act_remarks) as remarks,bc.charge_head, chc.chargehead_name,bc.act_department_id,dep.dept_name, pr.doctor,doc.doctor_name, bc.act_rate,bc.act_quantity,bc.amount,bc.discount,b.username
from bill b 
join bill_charge bc on (b.bill_no = bc.bill_no) 
join patient_registration pr on (pr.patient_id = b.visit_id)
join department dep on (dep.dept_id = bc.act_department_id)
join doctors doc on (doc.doctor_id = pr.doctor)
join chargehead_constants chc on ( chc.chargehead_id = bc.charge_head ) 

where b.status ='X' and
doc.doctor_id in ($P!{doctorId}) and   b.username in ($P!{userName})  and
bc.act_department_id in ($P!{departmentId}) and 
 to_char(b.finalized_date,'yyyy-mm-dd') BETWEEN '$P!{fromdate}' AND '$P!{todate}'

union  

select pr.mr_no, b.bill_no, b.visit_id, b.bill_type,to_date(to_char(b.open_date,'dd-mm-yyyy'),'dd-mm-yyyy') as open_date,coalesce(b.remarks,bc.act_remarks)as remarks,bc.charge_head, chc.chargehead_name,bc.act_department_id, dep.dept_name, pr.doctor,doc.doctor_name, bc.act_rate,bc.act_quantity,bc.amount,bc.discount,b.username
from bill_charge bc 
join bill b on (b.bill_no=bc.bill_no)
join patient_registration pr on (pr.patient_id = b.visit_id)
join department dep on (dep.dept_id = bc.act_department_id)
join doctors doc on (doc.doctor_id = pr.doctor)
join chargehead_constants chc on ( chc.chargehead_id = bc.charge_head ) 
where bc.status ='X' and
doc.doctor_id in ($P!{doctorId}) and  b.username in ($P!{userName})  and
bc.act_department_id in ($P!{departmentId}) and 
 to_char(bc.mod_time,'yyyy-mm-dd') BETWEEN '$P!{fromdate}' AND '$P!{todate}'
 order by username,dept_name, doctor, open_date]]>
	</queryString>
