	<queryString>
		<![CDATA[SELECT gd.dept_name,s.supplier_name,s.cust_supplier_code,s.drug_license_no,s.pan_no,s.cin_no, pi.invoice_no, gm.grn_date,gm.grn_no,hcm.center_name,
		case when discount_type = 'A' then coalesce((pi.discount-pi.round_off),0) when
		discount_type = 'P' then
		coalesce((((select sum(g1.billed_qty/g1.grn_pkg_size*g1.cost_price - g1.discount + g1.tax)
		FROM store_grn_details g1 JOIN store_grn_main gm1 on
		g1.grn_no = gm1.grn_no JOIN stores gd ON (gd.dept_id=gm1.store_id) where gm1.supplier_invoice_id = pi.supplier_invoice_id )
		+ pi.other_charges + (pi.cess_tax_rate * (sum(g.tax)) / 100))*pi.discount_per/100) - pi.round_off,0) end as deductions,
		pi.tcs_amount, sum(g.cost_price*g.billed_qty/g.grn_pkg_size)
		as amount,
			sum(g.tax) as tax, sum(g.discount) as discount,sum(g.scheme_discount) as scheme_discount,
			g.tax_rate,pi.cess_tax_rate,
			(pi.cess_tax_rate * sum(g.tax) / 100) as cess_tax_amt,
			pi.other_charges,sum(g.item_ced) as ced
	        FROM store_grn_details g JOIN store_grn_main gm using(grn_no)
	        JOIN store_invoice pi USING (supplier_invoice_id)
	        JOIN supplier_master s on (s.supplier_code=pi.supplier_id)
	        JOIN stores gd ON (gd.dept_id=gm.store_id)
	        JOIN store_type_master sty ON (gd.store_type_id =sty.store_type_id)
	        LEFT JOIN hospital_center_master hcm ON (hcm.center_id = gd.center_id)
        where ($P{supplier_id} = '*' OR pi.supplier_id =
		$P{supplier_id}) AND ($P{store_id}= '*' OR gm.store_id::character varying=$P{store_id}) AND
		($P{store_type_id} = '*' OR gd.store_type_id::character varying = $P{store_type_id}) AND
		 date_trunc('day',gm.grn_date) >= $P{fromDate}  AND
		date_trunc('day',gm.grn_date) <= $P{toDate} $P!{centerClause}
        GROUP BY gd.dept_name,pi.supplier_invoice_id,s.supplier_name,s.cust_supplier_code,s.drug_license_no,s.pan_no,s.cin_no,pi.supplier_id,pi.invoice_no, pi.cess_tax_rate, pi.cess_tax_amt,
                g.tax_rate,gm.grn_date,gm.grn_no,pi.discount,pi.round_off,pi.discount_type,pi.discount_per,pi.other_charges,hcm.center_name
        ORDER BY gd.dept_name,s.supplier_name,pi.invoice_no,pi.cess_tax_rate,
                pi.cess_tax_amt, gm.grn_date]]>
	</queryString>
