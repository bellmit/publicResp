	<queryString>
		<![CDATA[select distinct adj_detail_no, s.dept_name as store,
i.medicine_name as item_name,
sibd.batch_no as item_identifier,
to_char(am.date_time, 'DD-MM-YYYY HH:MI AM')as date,
am.username,
a.qty as quantity,
am.reason,
am.adj_no,
'Stock Change' as type,
description,
case when issue_type='P' then 'Permanent' when issue_type='C' then 'Consumable' when issue_type= 'R' then 'Retailable' else 'Reusable' end as issue_type,
case when consignment_stock='t' then 'Consignment' else 'Normal' end as stocktype
from store_adj_main am
join store_adj_details a using(adj_no)
JOIN store_item_batch_details sibd USING(item_batch_id)
join store_item_details  i on(i.medicine_id = a.medicine_id)
join store_category_master scm on(scm.category_id = i.med_category_id)
join stores s on(s.dept_id = am.store_id)
join store_stock_details imsd on (imsd.dept_id=am.store_id and imsd.medicine_id=a.medicine_id and a.item_batch_id=imsd.item_batch_id)
where am.adj_no='$P!{adjNo}']]>
