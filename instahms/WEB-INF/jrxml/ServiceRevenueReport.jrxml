	<queryString>
		<![CDATA[SELECT pd.mr_no, b.visit_id, b.bill_no, sdep.department,
  get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as patient_name,
  serv.service_name as act_description,
  bc.amount, (bc.act_rate*bc.act_quantity) as act_rate, coalesce(bc.discount,0) as discount
FROM bill_charge bc
  JOIN bill b using(bill_no)
  LEFT JOIN patient_registration pr ON (pr.patient_id = b.visit_id)
  LEFT JOIN patient_details pd ON (pd.mr_no = pr.mr_no)
  JOIN bill_activity_charge bac ON (bac.charge_id = bc.charge_id)
  LEFT JOIN services_prescribed sp ON (sp.prescription_id::varchar = bac.activity_id)
  JOIN services serv ON (serv.service_id = bc.act_description_id)
  JOIN services_departments sdep ON (sdep.department = serv.dept_name)
  LEFT JOIN sample_collection sc ON (sc.pprescribed_id = sp.prescription_id)
WHERE bc.charge_group='SNP' and bc.status!='X' and b.status in ('F','S','C') and
  date(b.finalized_date) between $P{fromdate} and $P{todate} $P!{deptId}
ORDER BY sdep.department]]>
	</queryString>
