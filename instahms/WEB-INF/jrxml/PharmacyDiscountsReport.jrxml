	<parameter name="deptId" class="java.lang.String"/>
	<queryString>
		<![CDATA[select bill_no,sale_date,bc.amount,bc.discount,b.visit_id,
coalesce(pd.patient_name,prc.customer_name) as patname,pr.mr_no,sale_id,
case when prc.is_credit='Y' and b.visit_type='r' then 'Retail Credit' 
when prc.is_credit='N' and b.visit_type='r' then 'Retail'
when  b.visit_type='o' then 'Out Patient' else 'In Patient' end as patient_type,gd.dept_name
from pharmacy_medicine_sales_main 
join  general_departments gd on store_id=gd.dept_id
join bill b using(bill_no) 
join bill_charge bc using(bill_no) 
left join patient_registration pr on(pr.patient_id=b.visit_id)
left join patient_details pd on(pr.mr_no=pd.mr_no)
left join pharmacy_retail_customers prc on(prc.customer_id=b.visit_id) 
join bill_activity_charge bac on bac.activity_id=sale_id::varchar and bac.charge_id=bc.charge_id 
where date(sale_date) >= $P{fromDate} AND date(sale_date) <=$P{toDate}
and bc.discount!=0 AND ($P{deptId}= '' OR gd.dept_id = $P{deptId}) 
order by gd.dept_name,patient_type,sale_id desc]]>
	</queryString>
