  s.tax_rate, s.quantity, s.amount, s.rate, s.package_unit, b.bill_type, b.restriction_type
 FROM store_sales_main sm
  JOIN store_sales_details s USING (sale_id)
  JOIN store_item_details m ON (s.medicine_id = m.medicine_id)
  JOIN manf_master mf ON (m.manf_name = mf.manf_code)
  JOIN bill b ON (sm.bill_no = b.bill_no)
 WHERE sm.bill_no = $P{billNo}
 ORDER by sm.sale_id, s.sale_item_id]]>
