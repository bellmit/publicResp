	<queryString>
		<![CDATA[SELECT bc.posted_date, chc.chargehead_name, cgc.chargegroup_name, act_description, act_remarks,
		act_rate_plan_item_code, charge_id, amount+tax_amt as amount, tax_amt, 
		CASE WHEN b.is_tpa THEN COALESCE(bc.sponsor_tax_amt, 0) ELSE 0 END AS sponsor_tax_amt, 
		bc.discount, bc.act_quantity, bc.act_rate, bc.act_unit,
		CASE WHEN b.is_tpa THEN COALESCE(bc.insurance_claim_amount, 0) ELSE 0 END AS insurance_claim_amount,
		b.bill_type
FROM bill_charge bc
 JOIN bill b USING (bill_no)
 JOIN chargegroup_constants cgc ON cgc.chargegroup_id=bc.charge_group
 JOIN chargehead_constants chc ON chc.chargehead_id=bc.charge_head
WHERE b.bill_no = $P{billNo} AND bc.status!='X' AND bc.charge_head != 'BIDIS'
 AND ($P{chargeGroup} = 'ALL' OR $X{IN, bc.charge_head ,chargeHead})
 AND (($P{detailed} = 'DET') OR
      ($P{detailed} = 'INS' AND bc.insurance_claim_amount != 0) OR
      ($P{detailed} = 'PAT' AND (bc.amount - bc.insurance_claim_amount) != 0)
     ) AND (act_rate !=0 OR discount !=0)
ORDER by b.bill_no, cgc.display_order, chc.display_order, bc.posted_date]]>
	</queryString>
