	<parameter name="store" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT SUM(qty) AS qty,medicine_name as item_name,test_name AS name,
(SELECT count(consumer_id) from store_reagent_usage_main where consumer_id=d.test_id group by d.test_id) as count,
case when tp.conducted='C' then 'Conducted'
when tp.conducted='N' then 'Not Conducted'
when tp.conducted='P' then 'Partially Conducted'
when tp.conducted='X' then 'Cancelled'
when tp.conducted='Y' then 'SignOff' end  AS status
from store_reagent_usage_details
join store_reagent_usage_main rm using(reagent_usage_seq)
join diagnostics d on test_id=consumer_id
join tests_conducted tc on prescribed_id=rm.ref_no and consumer_id=tc.test_id
left join tests_prescribed tp on tp.prescribed_id=rm.ref_no and consumer_id=tp.test_id
left join store_item_details itd using (medicine_id)
where store_id::varchar=$P{store}
and date(conducted_date) >= $P{fromDate} AND date(conducted_date) <=$P{toDate}
group by medicine_name,d.test_id,test_name,tp.conducted
union all
select sum(qty) as qty,medicine_name as item_name,service_name as name,
(select count(consumer_id) from store_reagent_usage_main where consumer_id=s.service_id group by s.service_id) as count,
case when sp.conducted='C' then 'Conducted'
when sp.conducted='N' then 'Not Conducted'
when sp.conducted='P' then 'Partially Conducted'
when sp.conducted='X' then 'Cancelled' end as status
from  store_reagent_usage_details
join store_reagent_usage_main rm using(reagent_usage_seq)
join services s on service_id=consumer_id
join services_prescribed sp on prescription_id=rm.ref_no and consumer_id=sp.service_id
left join store_item_details itd using (medicine_id)
where store_id::varchar=$P{store}
and date(conducteddate) >= $P{fromDate} AND date(conducteddate) <=$P{toDate}
group by medicine_name,s.service_id,service_name,sp.conducted
order by item_name]]>
	</queryString>
