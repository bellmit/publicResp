	<queryString>
		<![CDATA[SELECT bill_no, bill_type, b.status, visit_id, open_date,
finalized_date, deposit_set_off, total_claim,b.procedure_no,procedure_code
,procedure_name,procedure_limit,COALESCE(claim_recd_amount,0) AS claim_recd_amount,sponsor_bill_no,
 COALESCE(insurance_deduction,0) AS insurance_deduction,pr.primary_sponsor_id AS tpa_id
FROM bill b
left join patient_registration pr on(b.visit_id=pr.patient_id)
LEFT OUTER JOIN sponsor_procedure_limit using(procedure_no)
WHERE mr_no = $P{mr_no} AND date_trunc('day',finalized_date) BETWEEN $P{fromDate} AND  $P{toDate}
ORDER BY finalized_date]]>
