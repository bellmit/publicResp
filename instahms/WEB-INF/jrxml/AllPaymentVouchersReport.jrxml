	<parameter name="paymentCategory" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT CASE WHEN pd.payment_type='C' THEN 'Cash Voucher' WHEN pd.payment_type='D' THEN
'Doctor Payments' WHEN pd.payment_type in ('R','F') THEN 'Referral Doctor Payments' WHEN pd.payment_type='O'
THEN 'Out Hospital Payments' WHEN pd.payment_type = 'P' THEN 'Prescribing Doctor' WHEN pd.payment_type = 'S' THEN 'Supplier'  END as payment_type,
p.voucher_no, p.type,pd.amount, p.tax_type,p.tax_amount,p.date,p.counter,p.username,p.reference_no,
p.tds_amount, CASE WHEN pd.payment_type in ('D','P') THEN (SELECT doctor_name from doctors where
doctor_id=p.payee_name) WHEN pd.payment_type='R' THEN (SELECT doctor_name from doctors
where doctor_id=p.payee_name) WHEN pd.payment_type='F' THEN (SELECT referal_name FROM referral
WHERE referal_no=p.payee_name) WHEN pd.payment_type='O' THEN (SELECT oh_name FROM  outhouse_master where oh_id=pd.payee_name)
 WHEN pd.payment_type='S' THEN (SELECT supplier_name FROM  supplier_master where supplier_code=pd.payee_name) ELSE p.payee_name END as payee_name,  p.payment_mode_id, pm.payment_mode as mode, p.card_type_id, ctm.card_type,p.bank ,
pd.payment_type,pd.description,pd.category
FROM payments p
	JOIN payments_details pd  on (p.payee_name=pd.payee_name and pd.voucher_no=p.voucher_no )
	JOIN payment_mode_master pm on (pm.mode_id = p.payment_mode_id)
	LEFT JOIN card_type_master ctm on (ctm.card_type_id =  p.card_type_id)
where pd.voucher_no is not null $P!{screen} $P!{paymentType}  $P!{paymentCategory}  $P!{payeeId}  $P!{payDate}
order by pd.payment_type]]>
	</queryString>
