	<queryString>
		<![CDATA[SELECT pd.mr_no,
 get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as full_name,
 pd.patient_gender, pd.patient_address, c.city_name, stm.state_name,
 get_patient_age(pd.dateofbirth, pd.expected_dob) as patient_age,
 get_patient_age_in(pd.dateofbirth, pd.expected_dob) as patient_age_in,
 pr.patient_id, pr.reg_date, pr.reg_time, pr.visit_type, pr.discharge_date, pr.discharge_time,
 dep.dept_name, doc.doctor_name, od.org_name, tpa.tpa_name,
 pr.bed_type as bill_bed_type, wn.ward_name as reg_ward_name,
 bn.bed_name as alloc_bed_name, wn.ward_name as alloc_ward_name,
 rdoc.doctor_name as referred_by, rf.referal_name as other_referal,
 dtm.discharge_type,
 icm.insurance_co_name, ic.policy_no, ic.insurance_id, ic.policy_holder_name,
 ic.insurance_no, ipm.plan_name

FROM patient_registration pr
 JOIN patient_details pd ON (pr.mr_no=pd.mr_no)
 JOIN city c ON (c.city_id=pd.patient_city)
 JOIN state_master stm ON (stm.state_id=pd.patient_state)
 LEFT JOIN department dep ON (dep.dept_id=pr.dept_name)
 LEFT JOIN doctors doc ON (doc.doctor_id=pr.doctor)
 LEFT JOIN organization_details OD ON (pr.org_id=od.org_id)
 LEFT JOIN tpa_master tpa ON (pr.primary_sponsor_id=tpa.tpa_id)
 LEFT JOIN doctors rdoc ON (rdoc.doctor_id=pr.reference_docto_id)
 LEFT JOIN referral rf ON (rf.referal_no=pr.reference_docto_id)
 LEFT JOIN discharge_type_master dtm ON(pr.discharge_type_id = dtm.discharge_type_id)
 LEFT JOIN ip_bed_details ibd on (ibd.patient_id=pr.patient_id ) AND (ibd.status='A' or ibd.status='C')
 LEFT JOIN bed_names bn on (bn.bed_id=ibd.bed_id)
 LEFT JOIN ward_names wn on (wn.ward_no=bn.ward_no)
 LEFT JOIN insurance_company_master icm ON icm.insurance_co_id = pr.primary_insurance_co
 LEFT JOIN insurance_case ic on (ic.insurance_id=pr.insurance_id)
 LEFT JOIN insurance_plan_main ipm ON (ipm.plan_id = pr.plan_id)
WHERE pr.patient_id = $P{patientid}

UNION

SELECT null as mr_no, (prc.customer_name) as full_name,
 null as patient_gender, null as patient_address, null as city_name, null as state_name,
 null as patient_age, null as patient_age_in, (prc.customer_id)as patient_id, null as reg_date,
 null as reg_time, null as visit_type, null as discharge_date, null as discharge_time, null as dept_name,
 null as doctor_name, null as org_name, null as tpa_name, null as bill_bed_type, null as reg_ward_name,
 null as alloc_bed_name, null as alloc_ward_name, null as referred_by, null as other_referal,
 null as discharge_type, null as insurance_co_name,
 null as policy_no, null as insurance_id,  null as policy_holder_name,
 null as insurance_no,null as plan_name
FROM  store_retail_customers prc
WHERE  prc.customer_id =$P{patientid}

UNION

SELECT null as mr_no, isr.patient_name as full_name,
 isr. patient_gender, null as patient_address, null as city_name, null as state_name,
 isr.patient_age ,'Y' as patient_age_in, (isr.incoming_visit_id)as patient_id, null as reg_date,
 null as reg_time,'' as visit_type, null as discharge_date, null as discharge_time, null as dept_name,
 null as doctor_name, null as org_name, null as tpa_name, null as bill_bed_type, null as reg_ward_name,
 null as alloc_bed_name,
 null as alloc_ward_name, null as referred_by, null as other_referal,
 null as discharge_type,  null as insurance_co_name,
 null as policy_no, null as insurance_id,  null as policy_holder_name,
 null as insurance_no,null as plan_name
FROM  incoming_sample_registration isr
WHERE  isr.incoming_visit_id =$P{patientid}]]>
