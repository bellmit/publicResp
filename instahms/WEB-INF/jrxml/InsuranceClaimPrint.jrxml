	<queryString>
		<![CDATA[SELECT distinct b.charge_id, b.bill_no, b.charge_group, b.charge_head, b.act_department_id,
            b.act_description, b.act_remarks, b.act_rate, b.act_unit, b.act_quantity,
            b.amount, b.discount, b.discount_reason, b.charge_ref, b.paid_amount, b.posted_date,
            b.status, b.username, b.mod_time, b.approval_id, b.orig_rate, b.package_unit,
            b.doctor_amount, b.doc_payment_id, b.ref_payment_id, b.oh_payment_id,
            b.act_description_id, b.hasactivity, CASE WHEN b.insurance_claim_amount is null THEN 0.00 ELSE   b.insurance_claim_amount end, chargegroup_name, chargehead_name, dept_name,
 	 case when bil.status='A' then 'Provisional Insurance Claim'
	when bil.status = 'F' or bil.status = 'S' or bil.status = 'C'  then 'Final Insurance Claim' end as title
	FROM bill bil,bill_charge b LEFT OUTER JOIN department  ON
	(b.act_department_id = department.dept_id) JOIN chargehead_constants ON
	(b.charge_head = chargehead_constants.chargehead_id)  JOIN chargegroup_constants ON
	(b.charge_group = chargegroup_constants.chargegroup_id)
	WHERE b.bill_no ='$P!{billNo}'  and b.status != 'X' and bil.bill_no = b.bill_no]]>
	</queryString>
