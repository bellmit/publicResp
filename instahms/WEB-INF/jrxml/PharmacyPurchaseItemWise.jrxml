	<queryString>
		<![CDATA[SELECT u.*, $P!{groupBy} as group_name,
round((billed_qty*rate-item_discount+item_tax),2) as item_amt
FROM
(SELECT 'Purchase'::text AS purchase_type, sm.supplier_name, i.invoice_no, i.invoice_date,
  CASE WHEN i.status='O' THEN 'Open' WHEN i.status='F' THEN 'Finalized' ELSE 'Closed' END AS invoice_status,
  i.due_date, i.paid_date, i.tax_name, i.discount, i.round_off, i.other_charges, i.cess_tax_amt as cess,
  g.cost_price as rate, g.discount AS item_discount, g.tax_rate as item_tax_rate, g.tax as item_tax,
  m.medicine_name, g.batch_no, g.bonus_qty, g.billed_qty
FROM store_invoice i
  JOIN store_grn_main gm USING (supplier_invoice_id)
  JOIN store_grn_details g USING (grn_no)
  JOIN supplier_master sm ON (sm.supplier_code = i.supplier_id)
	JOIN store_item_details m USING (medicine_id)
UNION ALL
SELECT 'Debit Note'::text AS purchase_type, sm.supplier_name, d.debit_note_no, d.debit_note_date,
  CASE WHEN d.status = 'O' THEN 'Open' ELSE 'Closed' END AS invoice_status,
  date('1970-01-01') AS due_date, null AS paid_date, 'VAT' AS tax_name, (0-d.discount) AS discount,
  (0-d.round_off) AS round_off, (0-d.other_charges) as other_charges, 0 AS cess,
  g.cost_price, g.discount AS item_discount, g.tax_rate as item_tax_rate, g.tax as item_tax,
  m.medicine_name, g.batch_no, g.bonus_qty, g.billed_qty
FROM store_debit_note d
  JOIN store_grn_main gm USING (debit_note_no)
  JOIN store_grn_details g USING (grn_no)
	JOIN supplier_master sm ON (sm.supplier_code = d.supplier_id)
	JOIN store_item_details m USING (medicine_id)
) as u
WHERE date(invoice_date) BETWEEN $P{fromDate} AND $P{toDate}
 AND date(due_date) BETWEEN $P{dueFromDate} AND $P{dueToDate} AND
 ( ($P{filterValue} = '*') OR ($P!{filterBy}=$P{filterValue} )
   OR ($P{filterValue} = '' AND $P!{filterBy} IS NULL) )
ORDER BY group_name,invoice_date,invoice_no]]>
	</queryString>
