	<queryString>
		<![CDATA[select b.bill_no, coalesce( b.total_receipts,0.00) as paid,(b.total_amount) as amount,
		(b.total_claim) as insurance_claim_amount,(b.total_discount) as discount,b.bill_no,
 pr.discharge_date,get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as patient_name,b.deposit_set_off,
pr.patient_id ,pr.mr_no,pr.discharge_date,pr.visit_type,dtm.discharge_type, pr.referred_to, pr.reg_date,barv.payment_type,barv.recpt_type
From  patient_registration pr
LEFT OUTER JOIN bill b ON (b.visit_id = pr.patient_id AND b.bill_type = 'C' AND b.status != 'X' AND b.restriction_type = 'N')
LEFT OUTER JOIN bill_receipts barv using (bill_no)
join patient_details pd on pd.mr_no = pr.mr_no
LEFT JOIN discharge_type_master dtm ON(pr.discharge_type_id = dtm.discharge_type_id)
where pr.discharge_flag= 'D'   AND  pr.discharge_date  between $P{fromDate} and $P{toDate} and b.bill_no is not null
order by  pr.visit_type,barv.payment_type,barv.recpt_type,b.bill_no,pr.mr_no]]>
