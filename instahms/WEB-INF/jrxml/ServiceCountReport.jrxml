	<parameter name="serviceStatusArray" class="java.lang.String[]">
		<defaultValueExpression><![CDATA[new String[]{}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select count(sp.service_id), s.service_name, sd.department, sp.service_id,
 (select count(spres.patient_id) from services_prescribed spres
  join patient_registration pr on pr.patient_id = spres.patient_id
  where spres.service_id = sp.service_id and (pr.visit_type ='i')
 and  spres.presc_date::date between $P{fromDate} and $P{toDate}
 and $X{IN, spres.conducted, serviceStatusArray} )as "ip_count",
 (select count(spres.patient_id) from services_prescribed spres
 left join patient_registration pr on pr.patient_id = spres.patient_id
  where spres.service_id = sp.service_id and (pr.visit_type = 'o' or spres.patient_id like 'IN%')
and  date(spres.presc_date) between $P{fromDate} and $P{toDate}
and $X{IN, spres.conducted, serviceStatusArray})as "op_count"
from services_prescribed sp
inner join services s on (sp.service_id = s.service_id)
inner join services_departments sd on (s.dept_name = sd.department)
where date(sp.presc_date) between $P{fromDate} and $P{toDate}
AND $X{IN, sd.department ,deptIdArray}
AND $X{IN, sp.conducted , serviceStatusArray}
group by sp.service_id,s.service_name, sd.department order by  sd.department,count desc]]>
	</queryString>
