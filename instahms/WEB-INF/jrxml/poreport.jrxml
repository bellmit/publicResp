	<queryString>
		<![CDATA[SELECT DISTINCT SITD.MEDICINE_NAME,
POM.PO_NO,
TO_CHAR(POM.PO_DATE,'DD-MON-YY') AS PO_DATE,
SM.SUPPLIER_NAME,
REPLACE(SM.SUPPLIER_ADDRESS,'~','')AS SUPPLIER_ADDRESS,
COALESCE(SUPPLIER_CITY,'') as CITY_NAME,
COALESCE(SUPPLIER_COUNTRY,'') as COUNTRY_NAME,
COALESCE(SUPPLIER_STATE ,'') as STATE_NAME,
POM.SUPPLIER_TERMS,
POM.HOSPITAL_TERMS,
COALESCE(SM.SUPPLIER_TIN_NO,'')AS SUPPLIER_TIN,
PO.DISCOUNT,
PO.QTY_REQ,
PO.MRP,
PO.COST_PRICE,
PO.VAT_RATE,
PO.VAT,
POM.QUT_NO,
COALESCE(TO_CHAR(POM.QUT_DATE,'DD-MON-YY'),'') AS QUT_DATE,
PO.ADJ_MRP,
POM.REFERENCE,
PO.MED_TOTAL
FROM STORE_PO_MAIN POM JOIN STORE_PO PO ON POM.PO_NO = PO.PO_NO
JOIN SUPPLIER_MASTER SM on SM.SUPPLIER_CODE = POM.SUPPLIER_ID
JOIN STORE_ITEM_DETAILS SITD on PO.MEDICINE_ID = SITD.MEDICINE_ID
WHERE POM.PO_NO=$P{poNo}]]>
	</queryString>
