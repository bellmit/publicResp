		<![CDATA[select s.supplier_name,s.cust_supplier_code,s.drug_license_no,s.pan_no,s.cin_no,return_no,medicine_name,cust_item_code,sr.batch_no,
sr.qty,replaced_qty,(qty-replaced_qty) as remaingqty,
to_char(srm.date_time,'DD-MM-YYYY') as date
from store_supplier_returns_main srm
join store_supplier_returns sr using(return_no)
JOIN store_item_details sitd using(medicine_id)
JOIN supplier_master s on (s.supplier_code = supplier_id)
where orig_return_no is null and (qty-replaced_qty) > 0
