	<parameter name="debitNo" class="java.lang.String"/>
	<queryString>
		<![CDATA[select pdn.received_debit_amt,pdn.debit_note_no,s.supplier_name,TO_CHAR(pdn.debit_note_date,'dd-MM-yyyy') AS debit_note_date,
CASE WHEN pdn.discount_type = 'P' then ( select sdn1.discount_per * (0-sum((gd1.billed_qty/gd1.grn_pkg_size)*gd1.cost_price-gd1.discount+gd1.tax+(gd1.item_ced)))/100
from store_debit_note sdn1 join store_grn_main gm1 using (debit_note_no)
join store_grn_details gd1 using (grn_no)
group by sdn1.debit_note_no,sdn1.discount_per
having sdn1.debit_note_no = pdn.debit_note_no) ELSE pdn.discount END as rev_discount ,
CASE WHEN pdn.discount_type = 'P' then ( select sdn2.discount_per * (0-sum((png1.billed_qty/png1.grn_pkg_size)*png1.orig_debit_rate-png1.orig_discount+png1.orig_tax+(png1.item_ced)))/100
from store_debit_note sdn2 join store_grn_main gm2 using(debit_note_no)
join store_grn_details png1 using (grn_no)
group by sdn2.debit_note_no, sdn2.discount_per having sdn2.debit_note_no = pdn.debit_note_no) ELSE pdn.discount END as dbt_discount,
pdn.round_off,pdn.other_charges,pdn.other_charges_remarks,pdn.remarks,
mm.manf_mnemonic,mm.manf_name,medicine_name,png.batch_no,TO_CHAR(PNG.EXP_DT,'MON-YYYY') AS EXP_DATE,(0-((png.billed_qty/png.grn_pkg_size)*png.orig_debit_rate)) as amt,(0-(png.billed_qty/png.grn_pkg_size)) as qty,png.cost_price,
(0-png.orig_discount) as itemdiscount,png.tax_rate,(0-png.orig_tax) as tax,
(0-((png.billed_qty/png.grn_pkg_size)*png.orig_debit_rate-png.orig_discount+png.orig_tax+png.item_ced)) as totamt,(0-((png.billed_qty/png.grn_pkg_size)*png.cost_price-png.discount+png.tax+(png.item_ced))) as totrecamt, st.dept_name
from store_debit_note pdn
join supplier_master s on supplier_code=pdn.supplier_id
join store_grn_main pngm using(debit_note_no)
join store_grn_details png using(grn_no)
join store_item_details pmd using(medicine_id)
join manf_master mm on mm.manf_code=pmd.manf_name
join stores st on st.dept_id = pdn.store_id
where pdn.debit_note_no=$P{debitNo}]]>
	</queryString>
