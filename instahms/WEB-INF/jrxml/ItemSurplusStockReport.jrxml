	<queryString>
		<![CDATA[
select medicine_id,medicine_name,cust_item_code,sum(sold_qty)as sold_qty,sum(qty) as qty,store_name, avg(value_cp) as value_cp, avg(value_mrp) as value_mrp
from (
select pmsd.medicine_id,pms.medicine_name,pms.cust_item_code,0 as sold_qty,sum(pmsd.qty) as qty,s.dept_name AS store_name,
(round(pmsd.package_cp / pms.issue_base_unit, 2)*sum(pmsd.qty)) as value_cp, (round(sibd.mrp / pms.issue_base_unit, 2)*sum(pmsd.qty)) as value_mrp
 FROM store_stock_details pmsd
   JOIN store_item_batch_details sibd USING(item_batch_id)
   JOIN store_item_details pms ON (pms.medicine_id = pmsd.medicine_id)
   JOIN stores S USING (dept_id)
  
where NOT EXISTS
(SELECT s.medicine_id
FROM store_sales_details s
JOIN store_sales_main pmsm USING (sale_id)

where pmsd.medicine_id = s.medicine_id and date(pmsm.sale_date) between $P{fromDate} and  $P{toDate} 
GROUP BY s.medicine_id, s.batch_no, date(pmsm.sale_date))
and s.dept_id::character varying = $P{store_id}
GROUP BY pmsd.medicine_id, pms.medicine_name,pms.cust_item_code, sibd.batch_no, s.dept_id,
  pmsd.package_cp, pms.issue_base_unit, pmsd.qty, sibd.mrp
ORDER BY dept_name
) as foo 
 group by medicine_id, medicine_name ,cust_item_code,store_name]]>
