	<parameter name="fromDate" class="java.sql.Date" isForPrompting="false"/>
	<parameter name="wardnameArray" class="java.lang.String[]" isForPrompting="false">
		<defaultValueExpression><![CDATA[new String[]{}]]></defaultValueExpression>
	</parameter>
	<parameter name="mealnameArray" class="java.lang.String[]" isForPrompting="false"/>
	<parameter name="toDate" class="java.sql.Date" isForPrompting="false"/>
	<parameter name="center" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT dp.diet_id,dp.visit_id,pr.mr_no,coalesce ( bn.bed_name,'(Not allocated)') as bed_name,
 coalesce (wn.ward_name,'(Not allocated)') as ward_name,  dm.meal_name,dp.meal_timing,
 get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as patient_name,dp.ordered_id,dp.status,dp.meal_date,dp.special_instructions, dt.doctor_name
 FROM diet_prescribed dp
 JOIN patient_registration pr ON (pr.patient_id = dp.visit_id)
 LEFT JOIN diet_master dm ON (dm.diet_id = dp.diet_id)
 LEFT JOIN ip_bed_details ibd ON (ibd.patient_id = pr.patient_id AND ibd.status IN ('A','C'))
 LEFT JOIN bed_names bn ON (bn.bed_id = ibd.bed_id)
 LEFT JOIN ward_names wn ON (wn.ward_no = bn.ward_no)
 LEFT JOIN patient_diet_prescriptions pdp ON (dp.diet_pres_id = pdp.diet_pres_id)
 JOIN patient_details pd ON (pd.mr_no = pr.mr_no)
 LEFT JOIN doctors dt ON (dt.doctor_id = dp.ordered_by )
 WHERE dp.status = 'N'  AND date(dp.meal_date) BETWEEN $P{fromDate} AND $P{toDate}
AND $X{IN,wn.ward_no,wardnameArray}
AND $X{IN,dm.meal_name,mealnameArray}
AND ( $P!{center} = 0 OR $P!{center} = pr.center_id)
Order by ward_name,mr_no]]>
	</queryString>
