	<queryString>
		<![CDATA[SELECT bill_type, b.status, visit_id, open_date, finalized_date, chargegroup_name,
	CASE WHEN u.temp_username = '' THEN u.emp_username ELSE u.temp_username END as closed_by,
	COALESCE(deposit_set_off,0) AS deposit_set_off,restriction_type,total_claim,
	b.procedure_no,procedure_code,procedure_name,procedure_limit,
	total_receipts,COALESCE(primary_total_sponsor_receipts,0)+COALESCE(secondary_total_sponsor_receipts,0) AS total_sponsor_receipts
	,COALESCE(claim_recd_amount,0) AS claim_recd_amount,b.is_tpa,
	sponsor_bill_no, COALESCE(insurance_deduction,0) AS pat_ded, COALESCE(total_amount,0) AS total_amount
FROM bill b
	LEFT OUTER JOIN chargegroup_constants ON (chargegroup_id = $P{chargeGroup})
	LEFT OUTER JOIN sponsor_procedure_limit using(procedure_no)
	LEFT OUTER JOIN u_user u ON (u.emp_username = b.username)
WHERE bill_no = $P{billNo} limit 1]]>
