FROM store_purchase_invoice_report_view
WHERE date(invoice_date) BETWEEN $P{fromDate} AND $P{toDate}
 AND ( (purchase_type='Purchase' AND date(due_date) BETWEEN $P{dueFromDate} AND $P{dueToDate})
 	 OR (purchase_type = 'Debit Note' AND ($P{dueFromDate}= date('1970-01-01') ) ) )
