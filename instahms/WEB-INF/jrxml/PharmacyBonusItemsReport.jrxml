	<parameter name="medName" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT s.supplier_name,s.cust_supplier_code,s.drug_license_no,s.pan_no,s.cin_no, si.invoice_no, gm.grn_date, m.medicine_name,m.cust_item_code, sibd.batch_no,
		trunc(g.billed_qty,2) as billed_qty, trunc(g.bonus_qty,2) as bonus_qty,
		trunc(sibd.mrp/g.grn_pkg_size,$P{decimalplaces}) as mrp,
		trunc(g.cost_price/g.grn_pkg_size,$P{decimalplaces}) as cost_price,
		trunc((g.bonus_qty *(g.cost_price/g.grn_pkg_size)),$P{decimalplaces}) as amount,
		m.issue_base_unit
		FROM store_grn_details g JOIN store_grn_main gm USING (grn_no)
		JOIN store_item_batch_details sibd USING(item_batch_id)
		JOIN store_item_details m ON(m.medicine_id = g.medicine_id)
		JOIN store_invoice si USING (supplier_invoice_id)
		JOIN supplier_master s ON(s.supplier_code = si.supplier_id)
		WHERE ($P{supplier_id} = '' OR si.supplier_id = $P{supplier_id}) AND (DATE_TRUNC('DAY', gm.grn_date) BETWEEN  $P{fromDate}
		AND $P{toDate} ) AND g.bonus_qty > 0
AND ($P{medName} = '' or medicine_name=$P{medName})
ORDER BY supplier_name, gm.grn_date, si.invoice_no,$P{medName}]]>
