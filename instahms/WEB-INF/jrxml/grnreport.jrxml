	<queryString>
		<![CDATA[SELECT DISTINCT sitd.medicine_name,
sitd.issue_base_unit,
sgm.grn_no,
sgm.po_no,
TO_CHAR(sgm.grn_date,'DD-MON-YY') AS grn_date,
sgd.batch_no,
TO_CHAR(sgd.exp_dt,'MM-YYYY') AS exp_date,
sm.supplier_name,
REPLACE(sm.supplier_address,'~','')AS supplier_address,
COALESCE(supplier_city,'') AS city_name,COALESCE(supplier_country,'') AS country_name,
COALESCE(supplier_state,'') AS state_name,sinv.invoice_no,
((SELECT SUM(((sgd.billed_qty/sgd.grn_pkg_size * sgd.cost_price) - sgd.discount + sgd.tax)) AS amt
		FROM store_grn_details sgd,store_grn_main sgm
		 WHERE 	sgd.grn_no=sgm.grn_no
		AND sgm.supplier_invoice_id=sinv.supplier_invoice_id
		GROUP BY sgm.supplier_invoice_id) - sinv.discount + sinv.round_off ) AS final_amt,
COALESCE(sm.supplier_tin_no,'')AS supplier_tin,sgd.discount,sgd.bonus_qty,
TO_CHAR(sinv.invoice_date,'DD-MON-YY') AS invoice_date,
sgd.billed_qty,sgd.bonus_qty,sgd.mrp,sgd.cost_price,sgd.discount,sgd.outgoing_tax_rate as tax_rate,sgd.tax,
sgd.adj_mrp,sinv.po_reference,COALESCE(sinv.discount,0) AS invdisc,COALESCE(sinv.round_off,0) AS round_off,
((sgd.billed_qty/sgd.grn_pkg_size * sgd.cost_price) - sgd.discount + sgd.tax) AS amt,sgd.tax_type, sgm.user_name,s.dept_name as store_name
FROM store_grn_main sgm JOIN store_grn_details sgd ON sgm.grn_no = sgd.grn_no
JOIN store_invoice sinv USING (supplier_invoice_id)
JOIN supplier_master sm on sm.supplier_code = sinv.supplier_id
JOIN store_item_details sitd ON sgd.medicine_id = sitd.medicine_id
JOIN stores s on (s.dept_id=sgm.store_id)
WHERE sgm.grn_no=$P{grNo}]]>
	</queryString>
