	<queryString>
		<![CDATA[select * from (select medicine_id, medicine_name,sum(sold_qty) as sold_qty,sum(stock_qty) as stock_qty ,store_name,
case when sum(sold_qty)=0 THEN sum(value_cp) else avg(value_cp) end as value_cp,
case when sum(sold_qty)=0 THEN sum(value_mrp) else avg(value_mrp) end as value_mrp
from (
select medicine_id,medicine_name, sum(sold_qty) as sold_qty,stock_qty,s.dept_name AS store_name,
		         (unit_cp*sum(sold_qty)) as value_cp, (unit_mrp*sum(sold_qty)) as value_mrp
 from store_sales_details_view pmsv
 JOIN store_item_in_stock_view sv USING (medicine_id,batch_no,dept_id)
 JOIN stores s USING (dept_id)
 where  sales_date between  $P{fromDate} and  $P{toDate}
 and s.dept_id::character varying = $P{store_id}
 group by medicine_id,medicine_name,stock_qty,s.dept_name,unit_cp, unit_mrp
 ORDER BY s.dept_name
) as foo group by medicine_id, medicine_name, store_name) as foo1
 group by  medicine_id, medicine_name, store_name, sold_qty, stock_qty, value_cp, value_mrp having sold_qty  < $P{quantity} ::integer]]>
