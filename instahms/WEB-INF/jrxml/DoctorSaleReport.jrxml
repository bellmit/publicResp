	<queryString>
		<![CDATA[(select distinct(doctor_name) as doc,ptd.patient_name,to_char(pmsm.sale_date,'DD-MM-YYYY')as sale_date,b.bill_no,
bc.amount as amount,sale_id,gd.dept_name
from pharmacy_medicine_sales_main pmsm join bill b using(bill_no)
join  general_departments gd on pmsm.store_id=gd.dept_id
join bill_charge bc using(bill_no)
join patient_registration pd on b.visit_id=pd.patient_id
join patient_details ptd using (mr_no)
join bill_activity_charge bac on bac.activity_id=pmsm.sale_id::varchar and bac.charge_id=bc.charge_id
where bc.act_department_id='DEP_PHA' and date_trunc('day',pmsm.sale_date)>=$P{fromDate}
and date_trunc('day',pmsm.sale_date)<=$P{toDate} AND ($P{deptId}= '' OR gd.dept_id = $P{deptId})
order by dept_name,doctor_name,b.bill_no)
union
(select distinct(pmsm.doctor_name) as doc,prc.customer_name,to_char(pmsm.sale_date,'DD-MM-YYYY')as sale_date,b.bill_no,
bc.amount as amount ,sale_id,gd.dept_name
from pharmacy_medicine_sales_main pmsm join bill b using(bill_no)
join  general_departments gd on pmsm.store_id=gd.dept_id
join bill_charge bc using(bill_no)
join pharmacy_retail_customers prc on b.visit_id=prc.customer_id
join bill_activity_charge bac on bac.activity_id=pmsm.sale_id::varchar and bac.charge_id=bc.charge_id
where bc.act_department_id='DEP_PHA' and date_trunc('day',pmsm.sale_date)>=$P{fromDate}
and date_trunc('day',pmsm.sale_date)<=$P{toDate}  AND ($P{deptId}= '' OR gd.dept_id = $P{deptId})
order by dept_name,pmsm.doctor_name,b.bill_no)
order by  dept_name,doc,sale_date desc]]>
