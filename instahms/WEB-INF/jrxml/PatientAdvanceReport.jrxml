	<parameter name="filterValue" class="java.lang.String"/>
	<queryString>
		<![CDATA[select pr.patient_id,get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as patient_name,		pr.mr_no,pr.visit_type,brc.display_date,
	sum(brc.amount) as amount,brc.recpt_type,brc.bill_no,brc.receipt_no,brc.payment_mode_id,
	pm.payment_mode, brc.card_type_id, ctm.card_type, pm.bank_required, pm.ref_required
from  patient_registration pr
join  patient_details  pd on(pr.mr_no = pd.mr_no)
join bill b on (b.visit_id = pr.patient_id)
join bill_receipts brc on (brc.bill_no = b.bill_no and recpt_type = 'A')
join payment_mode_master pm on (brc.payment_mode_id = pm.mode_id)
left join card_type_master ctm on (brc.card_type_id = ctm.card_type_id)
where date(display_date ) between  $P{fromDate} and  $P{toDate} AND
( ($P{filterValue} = '*')
OR (pr.visit_type=$P{filterValue} ) OR ($P{filterValue} = '' AND pr.visit_type IS NULL) )
group by pr.patient_id,pd.patient_name,pd.last_name,pr.mr_no,brc.recpt_type,brc.bill_no,pr.visit_type,brc.display_date,brc.receipt_no,brc.payment_mode_id, pm.payment_mode, brc.card_type_id, ctm.card_type, pm.bank_required, pm.ref_required,
pd.salutation,pd.middle_name, pd.last_name
order by   pr.visit_type,brc.display_date,mr_no]]>
