	</parameter>
	<queryString>
		<![CDATA[SELECT * from (

		SELECT DISTINCT si.supplier_id,supplier_name,cust_supplier_code,si.invoice_no,to_char(si.invoice_date,'dd-MM-yyyy') AS invoice_date,
		TO_CHAR(due_date,'dd-MM-yyyy') AS due_date,	CASE WHEN si.status='O' THEN 'Open' WHEN si.status='C' THEN 'Closed'
		WHEN si.status='F' THEN 'Finalized' END AS status,CASE WHEN paid_date IS NULL THEN ''
		ELSE TO_CHAR(paid_date,'dd-MM-yyyy') END AS paid_date, payment_remarks,
		(SELECT SUM(((sgd.billed_qty/sgd.grn_pkg_size * sgd.cost_price) - (sgd.discount+sgd.scheme_discount) + sgd.tax))
		FROM store_grn_details sgd,store_grn_main sgm,store_item_details sid WHERE sgd.grn_no=sgm.grn_no
		AND sgd.medicine_id = sid.medicine_id AND sgm.supplier_invoice_id=si.supplier_invoice_id
		GROUP BY sgm.supplier_invoice_id)+si.tcs_amount AS amt, si.tcs_amount, si.discount_type,si.discount_per,
		si.discount, si.round_off, coalesce(si.other_charges,0) as other_charges, si.cess_tax_rate, si.cess_tax_amt
		FROM store_grn_main sgm JOIN store_invoice si USING (supplier_invoice_id)
		JOIN supplier_master ON (si.supplier_id = supplier_code)
		WHERE DATE_TRUNC('DAY',si.due_date)>=$P{fromDate} AND DATE_TRUNC('DAY',si.due_date)<=$P{toDate}
		AND ($P{supplier} = '' OR si.supplier_id = $P{supplier})
                AND (($X{IN,si.status,invstatausArray}))
	UNION ALL
		SELECT DISTINCT supplier_id,supplier_name,cust_supplier_code,sdn.debit_note_no as invoice_no,TO_CHAR(sdn.debit_note_date,'dd-MM-yyyy') AS invoice_date,
		TO_CHAR(sdn.debit_note_date,'dd-MM-yyyy') AS due_date,CASE WHEN sdn.status='O' then 'Open' WHEN sdn.status='C' THEN 'Closed' END AS status,
		NULL AS paid_date, remarks,
		(SELECT SUM(((sgd.billed_qty/sgd.grn_pkg_size * sgd.cost_price) - (sgd.discount+sgd.scheme_discount) + sgd.tax))
		FROM store_grn_details sgd,store_grn_main sgm, store_item_details sid WHERE sgd.grn_no=sgm.grn_no AND sgd.medicine_id = sid.medicine_id
		 AND sgm.debit_note_no=sdn.debit_note_no
		GROUP BY supplier_id,debit_note_no) AS amt, 0 as tcs_amount,
		sdn.discount_type,sdn.discount_per,-sdn.discount, -sdn.round_off, coalesce(-sdn.other_charges,0) as other_charges,
		0 as cess_tax_rate, 0 as cess_tax_amt
		FROM store_debit_note sdn
		JOIN store_grn_main sgm ON (sgm.debit_note_no = sdn.debit_note_no )
		JOIN supplier_master ON (supplier_code = supplier_id)
		WHERE DATE_TRUNC('DAY',sdn.debit_note_date)>=$P{fromDate} AND DATE_TRUNC('DAY',sdn.debit_note_date)<=$P{toDate}
  		AND ($P{supplier} = '' OR supplier_id = $P{supplier})
                AND (($X{IN,sdn.status,invstatausArray}))
  		) supplier_payments ORDER BY supplier_id, invoice_date]]>
	</queryString>
