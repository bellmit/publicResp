name="purchasevatreport" pageWidth="700" pageHeight="842" whenNoDataType="NoDataSection" scriptletClass="com.insta.hms.common.JRCommonScriptlet"
columnWidth="640" leftMargin="30" rightMargin="30" topMargin="40" bottomMargin="40">
	<parameter name="fromDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="decimalplaces" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="store_id_Array" class="java.lang.Integer[]"/>
	<parameter name="store_type_id_Array" class="java.lang.Integer[]"/>
	<queryString>
		<![CDATA[SELECT sgd.tax_rate,  sum((cost_price*billed_qty/sgd.grn_pkg_size)+sgd.item_ced) AS purchase_amount,
	sum(sgd.tax) AS tax_amount, sum((cost_price*billed_qty/sgd.grn_pkg_size)+sgd.tax+sgd.item_ced) AS total_amount,
	sum(adj_mrp*billed_qty/sgd.grn_pkg_size) AS mrp, sum(sgd.discount) AS discount,sum(sgd.scheme_discount) AS scheme_discount,
	round(sum(CASE
		WHEN sgd.tax_type='MB' THEN bonus_qty/sgd.grn_pkg_size*adj_mrp*sgd.tax_rate/100
		WHEN sgd.tax_type='CB' AND billed_qty != 0 THEN
				bonus_qty*(cost_price-round(((sgd.discount+sgd.scheme_discount)/billed_qty/sgd.grn_pkg_size),2))*sgd.tax_rate/100
		ELSE 0.00
	END),2) AS btax,
	(SELECT sum(coalesce(si.discount,sdn.discount)-coalesce(si.round_off,sdn.round_off))
	FROM store_grn_main sgm
		LEFT JOIN store_invoice si using(supplier_invoice_id)
left join store_debit_note sdn on sdn.debit_note_no = sgm.debit_note_no
LEFT JOIN stores st ON st.dept_id = sgm.store_id
LEFT JOIN store_type_master stm ON stm.store_type_id = st.store_type_id
	WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
	AND ($X{IN, sgm.store_id, store_id_Array})
	AND ($X{IN, st.store_type_id, store_type_id_Array})
	) AS deductions,
	(SELECT sum(si.tcs_amount)
	FROM store_grn_main sgm
		LEFT JOIN store_invoice si using(supplier_invoice_id)
left join store_debit_note sdn on sdn.debit_note_no = sgm.debit_note_no
LEFT JOIN stores st ON st.dept_id = sgm.store_id
LEFT JOIN store_type_master stm ON stm.store_type_id = st.store_type_id
	WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
	AND ($X{IN, sgm.store_id, store_id_Array})
	AND ($X{IN, st.store_type_id, store_type_id_Array})
	) AS tcs_amount,
	st.dept_name,
	stm.store_type_name
FROM  store_grn_details sgd
	JOIN store_grn_main sgm USING(grn_no)
	LEFT JOIN store_invoice si USING (supplier_invoice_id)
	LEFT JOIN store_item_details sid USING (medicine_id)
left join store_debit_note sdn on sdn.debit_note_no = sgm.debit_note_no
LEFT JOIN stores st ON st.dept_id = sgm.store_id
LEFT JOIN store_type_master stm ON stm.store_type_id = st.store_type_id
	JOIN supplier_master s ON (s.supplier_code = si.supplier_id or s.supplier_code=sdn.supplier_id)
WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
	AND ($X{IN, sgm.store_id, store_id_Array})
	AND ($X{IN, st.store_type_id, store_type_id_Array})
GROUP BY   sgd.tax_rate, st.dept_name, stm.store_type_name
ORDER BY   sgd.tax_rate]]>
	</queryString>
