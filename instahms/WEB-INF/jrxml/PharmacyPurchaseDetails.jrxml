	<queryString>
		<![CDATA[SELECT gd.dept_name,s.supplier_name,s.cust_supplier_code,s.drug_license_no,s.pan_no,s.cin_no, pi.invoice_no,gm.grn_date,gm.grn_no,g.item_ced as ced,
				m.medicine_name,m.cust_item_code, g.batch_no, g.billed_qty, g.bonus_qty,
				round(g.mrp/g.grn_pkg_size,2) as mrp, round(g.cost_price/g.grn_pkg_size,2) as cost_price, g.discount,g.scheme_discount, g.tax, g.tax_rate,
				(g.billed_qty/g.grn_pkg_size *g.cost_price - (g.discount+g.scheme_discount) + g.tax+g.item_ced) as amount,
				case when discount_type = 'A' then coalesce((pi.discount-pi.round_off),0) when
				discount_type = 'P' then
				coalesce(((select sum(g1.billed_qty/g1.grn_pkg_size*g1.cost_price - (g1.discount+g1.scheme_discount) + g1.tax ) + (sum(g1.tax) * pi.cess_tax_rate / 100 )
				FROM store_grn_details g1 JOIN store_grn_main gm1 on
				g1.grn_no = gm1.grn_no JOIN stores gd ON (gd.dept_id=gm1.store_id)
				where gm1.supplier_invoice_id = pi.supplier_invoice_id )
				+ pi.other_charges)*pi.discount_per/100 - pi.round_off,0) end as deductions,
				pi.other_charges,
				(select count(*) from store_grn_details where
				gm.grn_no=grn_no) as list, pi.cess_tax_rate, coalesce((select sum(g3.tax) * pi.cess_tax_rate / 100
				from store_grn_details g3 join store_grn_main gm3 on g3.grn_no = gm3.grn_no
				JOIN stores gd1 on (gd1.dept_id = gm3.store_id )
				where gm3.supplier_invoice_id = pi.supplier_invoice_id),0) as cess_tax_amt,hcm.center_name
				FROM store_grn_details g
				JOIN store_grn_main gm USING(grn_no)
				JOIN store_item_details m USING(medicine_id)
				JOIN store_invoice pi USING (supplier_invoice_id)
				JOIN supplier_master s ON(s.supplier_code = pi.supplier_id)
				JOIN stores gd ON (gd.dept_id=gm.store_id)
				JOIN store_type_master stm ON (gd.store_type_id = stm.store_type_id)
				LEFT JOIN hospital_center_master hcm ON (hcm.center_id = gd.center_id)
				WHERE ($P{supplier_id} = '*' OR pi.supplier_id = $P{supplier_id}) AND
				($P{store_id} = '*'  OR gm.store_id::character varying=$P{store_id}) AND
				($P{store_type_id} = '*' OR gd.store_type_id::character varying = $P{store_type_id}) AND
				(date_trunc('day', gm.grn_date) BETWEEN  $P{fromDate} AND $P{toDate} ) $P!{centerClause}
				ORDER BY dept_name,supplier_name, invoice_no, gm.grn_date]]>
	</queryString>
