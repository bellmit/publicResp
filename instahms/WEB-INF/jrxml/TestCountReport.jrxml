	</parameter>
	<queryString>
                <![CDATA[select count(tp.test_id), d.test_name,dd.category ,tp.test_id,
        count(pri.patient_id)  as ip_count,
        (count(pro.patient_id)+count(i.incoming_visit_id)) as op_count
        from tests_prescribed tp 
        left join patient_registration pri on (tp.pat_id=pri.patient_id and pri.visit_type ='i')
        left join patient_registration pro on (tp.pat_id=pro.patient_id and pro.visit_type ='o' )
        left join incoming_sample_registration i on (tp.pat_id=i.incoming_visit_id)
        inner join diagnostics d on (tp.test_id = d.test_id)
        inner join diagnostics_departments dd on (d.ddept_id = dd.ddept_id)
        where tp.pres_date between $P{fromDate} and $P{toDate}   
        AND $X{IN, dd.category, categoryArray}
        AND $X{IN, tp.conducted, testStatusArray} 
        AND $X{IN, d.ddept_id, deptIdArray} 
        group by tp.test_id,d.test_name,dd.category  order by  dd.category,count desc]]>
	</queryString>
