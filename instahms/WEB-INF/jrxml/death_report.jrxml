	<queryString>
		<![CDATA[select pr.mr_no ,get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name)  as Patient_name,
to_char(reg_date,'dd-mm-yyyy') as admit_date,
to_char(death_date,'dd-mm-yyyy') as deathdate,
to_char(death_time, 'HH:MI AM') as deathtime,
drm.reason as death_reason,d.doctor_name
from patient_registration pr
join patient_details pd on (pr.mr_no = pd.mr_no)
left join doctors d on (d.doctor_id = pr.doctor)
LEFT JOIN discharge_type_master dtm ON(pr.discharge_type_id = dtm.discharge_type_id)
LEFT JOIN death_reason_master drm ON (drm.reason_id=pd.death_reason_id)
where dtm.discharge_type = 'Death' and death_date between $P{fromDate} and $P{toDate}]]>
