	<parameter name="fromDate" class="java.sql.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[java.sql.Date.valueOf("2008-04-01")]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" class="java.sql.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[java.sql.Date.valueOf("2009-03-31")]]></defaultValueExpression>
