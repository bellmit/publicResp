	<queryString>
		<![CDATA[SELECT
  extract(year from pr.reg_date) AS year, extract(month from pr.reg_date) as month,
  d.dept_name, count(pr.patient_id) as count,0 as revenue
 FROM patient_registration pr
  JOIN department d on(pr.dept_name = d.dept_id)
 WHERE pr.visit_type = 'i' AND
	date(pr.reg_date) BETWEEN $P{fromDate} AND $P{toDate}
 GROUP BY year, month, d.dept_name

 union

 SELECT
  extract(year from b.finalized_date) AS year, extract(month from b.finalized_date) as month,
  d.dept_name, 0 as count,sum(bc.amount) as revenue
 FROM bill_charge bc
  JOIN bill b on (b.bill_no = bc.bill_no and b.status != 'X' and bc.status != 'X' and b.status != 'A'  )
  JOIN patient_registration pr on (b.visit_id = pr.patient_id)
       JOIN department d on(pr.dept_name = d.dept_id)
 WHERE pr.visit_type = 'i' AND
	date(b.finalized_date) BETWEEN $P{fromDate} AND $P{toDate}
 GROUP BY year, month, d.dept_name;]]>
	</queryString>
