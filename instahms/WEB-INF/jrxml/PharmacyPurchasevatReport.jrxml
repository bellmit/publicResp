	<parameter name="vatORcstTwo" class="java.lang.String"/>
	<queryString>
		<![CDATA[select pd.supplier_name,sum(pd.total_amount)as total_amount,sum(pd.tax_amount)as tax_amount,sum(pd.zeroper_taxamount) as zeroper_taxamount,
sum(pd.tax_amount_one) as tax_amount_one, sum(tax_amount_two) as tax_amount_two,sum( distinct(pd.taxone_taxrate)) as taxone_taxrate,sum(distinct(pd.taxtwo_taxrate))as taxtwo_taxrate
,sum(pd.deductions) as deductions,pd.invoice_no,pd.invoice_date,pd.supplier_tin_no,pd.remarks,pd.group_name from (
SELECT supplier_name, tax_rate, tax_type, sum(cost_price*billed_qty) AS purchase_amount,
sum(png.tax) AS tax_amount, sum((cost_price*billed_qty)-png.discount+png.tax) AS total_amount,
sum(adj_mrp*billed_qty) AS mrp, sum(png.discount) AS discount,
round(sum(CASE
WHEN tax_type='MB' THEN bonus_qty*adj_mrp*tax_rate/100
WHEN tax_type='CB' AND billed_qty != 0 THEN
bonus_qty*(cost_price-round((png.discount/billed_qty),2))*tax_rate/100
ELSE 0.00
END),2) AS btax,
CASE
WHEN tax_type='M' THEN 'MRP Based(without bonus)'
WHEN tax_type='MB' THEN 'MRP Based(with bonus)'
WHEN tax_type='C' THEN 'Cost Price Based(without bonus)'
ELSE 'Cost Price Based(with bonus)'
END AS vattype,
(SELECT sum(discount-round_off)
FROM pharmacy_new_grn_main pngm
LEFT JOIN  pharmacy_invoice pi using(supplier_invoice_id)
WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
) AS deductions,pi.invoice_no,pi.invoice_date,s.supplier_tin_no,pi.remarks,$P!{groupBy} as group_name,
CASE
WHEN ( ($P{vatORcstOne} = 'VAT' AND tax_rate=$P{taxRateOne} AND pi.tax_name='VAT' ) OR ($P{vatORcstOne} = 'CST' AND pi.cst_rate=$P{taxRateOne} AND pi.tax_name='CST') ) THEN sum(cost_price*billed_qty)
ELSE
0.00 END AS tax_amount_one,
CASE
WHEN (tax_rate=0 AND pi.tax_name='VAT') THEN sum(cost_price*billed_qty)
ELSE
0.00 END AS zeroper_taxamount,
 CASE
WHEN   ( ($P{vatORcstTwo} = 'VAT' AND tax_rate=$P{taxRateTwo} AND pi.tax_name='VAT' ) OR ($P{vatORcstTwo} = 'CST' AND pi.cst_rate=$P{taxRateTwo}) AND pi.tax_name='CST'  ) THEN sum(cost_price*billed_qty)
ELSE
0.00 END AS tax_amount_two,
CASE
WHEN  ( ($P{vatORcstOne} = 'VAT' AND tax_rate=$P{taxRateOne} AND pi.tax_name='VAT' ) OR ($P{vatORcstOne} = 'CST' AND pi.cst_rate=$P{taxRateOne} AND pi.tax_name='CST') ) THEN sum(png.tax)
ELSE
0.00 END AS taxone_taxrate,
CASE
WHEN ( ($P{vatORcstTwo} = 'VAT' AND tax_rate=$P{taxRateTwo} AND pi.tax_name='VAT') OR ($P{vatORcstTwo} = 'CST' AND pi.cst_rate=$P{taxRateTwo}) AND pi.tax_name='CST' ) THEN sum(png.tax)
ELSE
0.00 END AS taxtwo_taxrate
FROM  pharmacy_new_grn png
JOIN pharmacy_new_grn_main pngm USING(grn_no)
LEFT JOIN pharmacy_invoice pi USING (supplier_invoice_id)
JOIN supplier_master s ON (s.supplier_code = pi.supplier_id)
WHERE date(grn_date) BETWEEN  $P{fromDate} AND $P{toDate} AND
( (tax_rate = 0 AND pi.tax_name = 'VAT') OR ($P{vatORcstOne} = 'VAT' AND tax_rate = $P{taxRateOne} AND pi.tax_name = 'VAT')
	OR (($P{vatORcstTwo} = 'VAT' AND tax_rate = $P{taxRateTwo} AND pi.tax_name = 'VAT'))
 	OR ($P{vatORcstOne} = 'CST' AND cst_rate = $P{taxRateOne}) OR (($P{vatORcstTwo} = 'CST' AND cst_rate = $P{taxRateTwo}))
)
GROUP BY supplier_name,supplier_name, tax_type, tax_rate,pi.invoice_no,pi.invoice_date,supplier_tin_no,pi.remarks, cst_rate, pi.tax_name
) as pd
group by pd.group_name,pd.supplier_name,pd.invoice_no,pd.invoice_date,pd.supplier_tin_no,pd.remarks]]>
