	<queryString>
		<![CDATA[SELECT medicine_name, sum(quantity) as no_of_medicines, TO_CHAR(pmsm.sale_date,'DD-MM-YYYY') AS sale_date,doctor_name as doc,
		sum(bc.amount) as amount
		FROM pharmacy_medicine_sales_main pmsm
		JOIN pharmacy_medicine_sales pms using(sale_id)
		JOIN pharmacy_medicine_details pmd using(medicine_id)
		JOIN bill_activity_charge bac on (bac.activity_id=pmsm.sale_id::varchar AND 
      activity_code = 'PHS')
		JOIN bill_charge bc using(charge_id)
		WHERE DATE_TRUNC('day',pmsm.sale_date)>=$P{fromDate} AND DATE_TRUNC('day',pmsm.sale_date)<=$P{toDate}
		GROUP BY medicine_name,sale_date,doctor_name
		ORDER BY doctor_name,medicine_name,sale_date]]>
	</queryString>
