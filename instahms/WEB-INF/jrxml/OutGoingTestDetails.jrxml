	<parameter name="fromDate" class="java.util.Date"/>
	<parameter name="toDate" class="java.util.Date"/>
	<queryString>
		<![CDATA[select  tc.test_id,tc.patient_id,tc.mr_no,b.bill_no,bc.amount,ohd.oh_charge,ohm.oh_name,ohm.oh_id,d.test_name,
get_patient_name(pd.salutation, pd.patient_name, pd.middle_name, pd.last_name) as patient_name
from tests_conducted tc
inner join ohmaster_detail ohd on (ohd.oh_testid = tc.test_id)
inner join outhouse_master ohm on (ohd.oh_id = ohm.oh_id)
inner join bill b on (tc.patient_id = b.visit_id)
inner join bill_charge bc on (b.bill_no = bc.bill_no and bc.act_description_id = tc.test_id )
inner join diagnostics d on (tc.test_id = d.test_id)
inner join sample_collection sc on (tc.patient_id = sc.patient_id)
inner join patient_details pd on (tc.mr_no = pd.mr_no)
where d.house_status = 'O'  AND date(sc.sample_date)  BETWEEN  $P{fromDate} AND  $P{toDate}
order by ohm.oh_name]]>
	</queryString>
