	<queryString>
		<![CDATA[select pr.patient_id,extract (year from pr.reg_date) as year ,extract (month from pr.reg_date) as month,
count (pr.patient_id) as new ,0 as total_amount,revisit
from patient_registration  pr
where date(pr.reg_date) between  $P{fromDate}  and  $P{toDate}
and pr.visit_type = 'o'
group by pr.patient_id,year,month,revisit

union all

select ' ' as patient_id, extract (year from bc.posted_date) as year ,extract (month from bc.posted_date) as month,
0 as new ,sum(amount) as total_amount,revisit
from patient_registration  pr
 left join bill b on b.visit_id = pr.patient_id
 left join bill_charge bc on bc.bill_no = b.bill_no and b.status != 'X' and bc.status != 'X'
where date(bc.posted_date) between  $P{fromDate}  and  $P{toDate}
and pr.visit_type = 'o'
group by b.visit_id,year,month,revisit]]>
	</queryString>
