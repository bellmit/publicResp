<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="purchasevatreport" pageWidth="700" pageHeight="842" whenNoDataType="NoDataSection" columnWidth="640" leftMargin="30" rightMargin="30" topMargin="40" bottomMargin="40">
	<parameter name="fromDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="store_id_Array" class="java.lang.Integer[]"/>
	<parameter name="store_type_id_Array" class="java.lang.Integer[]"/>
	<queryString>
		<![CDATA[SELECT coalesce(png.tax_rate, sid.tax_rate) as tax_rate, coalesce(png.tax_type, sid.tax_type) as tax_type, sum((cost_price*billed_qty/png.grn_pkg_size)+png.item_ced) AS purchase_amount,
	sum(png.tax) AS tax_amount, sum((cost_price*billed_qty/png.grn_pkg_size)+png.tax+png.item_ced) AS total_amount,
	sum(adj_mrp*billed_qty/png.grn_pkg_size) AS mrp, sum(png.discount) AS discount,sum(png.scheme_discount) AS scheme_discount,
	round(sum(CASE
		WHEN coalesce(png.tax_type, sid.tax_type)='MB' THEN bonus_qty/png.grn_pkg_size*adj_mrp*coalesce(png.tax_rate, sid.tax_rate)/100
		WHEN coalesce(png.tax_type, sid.tax_type)='CB' AND billed_qty != 0 THEN
				bonus_qty*(cost_price-round(((png.discount+png.scheme_discount)/billed_qty/png.grn_pkg_size),2))*coalesce(png.tax_rate, sid.tax_rate)/100
		ELSE 0.00
	END),2) AS btax,
	CASE
		WHEN coalesce(png.tax_type, sid.tax_type)='M' THEN 'MRP Based(without bonus)'
		WHEN coalesce(png.tax_type, sid.tax_type)='MB' THEN 'MRP Based(with bonus)'
		WHEN coalesce(png.tax_type, sid.tax_type)='C' THEN 'Cost Price Based(without bonus)'
		ELSE 'Cost Price Based(with bonus)'
	END AS vattype,
	(SELECT sum(coalesce(pi.discount,pdn.discount)-coalesce(pi.round_off,pdn.round_off))
	FROM store_grn_main pngm
		LEFT JOIN  store_invoice pi using(supplier_invoice_id)
left join store_debit_note pdn on pdn.debit_note_no = pngm.debit_note_no
LEFT JOIN stores st ON (st.dept_id = pngm.store_id)
LEFT JOIN store_type_master stm ON (stm.store_type_id = st.store_type_id)
	WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
	AND ($X{IN, st.dept_id, store_id_Array})
	AND ($X{IN, stm.store_type_id, store_type_id_Array})
	) AS deductions,
	(SELECT sum(pi.tcs_amount)
	FROM store_grn_main pngm
		LEFT JOIN  store_invoice pi using(supplier_invoice_id)
left join store_debit_note pdn on pdn.debit_note_no = pngm.debit_note_no
LEFT JOIN stores st ON (st.dept_id = pngm.store_id)
LEFT JOIN store_type_master stm ON (stm.store_type_id = st.store_type_id)
	WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
	AND ($X{IN, st.dept_id, store_id_Array})
	AND ($X{IN, stm.store_type_id, store_type_id_Array})
	) AS tcs_amount,
	st.dept_name,
	stm.store_type_name
FROM  store_grn_details png
	JOIN store_grn_main pngm USING(grn_no)
	LEFT JOIN store_invoice pi USING (supplier_invoice_id)
	LEFT JOIN store_item_details sid USING (medicine_id)
left join store_debit_note pdn on pdn.debit_note_no = pngm.debit_note_no
	JOIN supplier_master s ON (s.supplier_code = pi.supplier_id or s.supplier_code=pdn.supplier_id)
LEFT JOIN stores st ON (st.dept_id = pngm.store_id)
LEFT JOIN store_type_master stm ON (stm.store_type_id = st.store_type_id)
WHERE date(grn_date) BETWEEN $P{fromDate} AND $P{toDate}
AND ($X{IN, st.dept_id, store_id_Array})
AND ($X{IN, stm.store_type_id, store_type_id_Array})
GROUP BY  coalesce(png.tax_type, sid.tax_type), coalesce(png.tax_rate, sid.tax_rate), st.dept_name, stm.store_type_name
ORDER BY  coalesce(png.tax_type, sid.tax_type) desc, coalesce(png.tax_rate, sid.tax_rate)]]>
	</queryString>
