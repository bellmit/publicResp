		<defaultValueExpression><![CDATA["BL000231"]]></defaultValueExpression>
	</parameter>
	<parameter name="curSym" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT cgc.chargegroup_name, cgc.display_order, sum(bc.discount) as discount, sum(bc.amount) as amount,
 sum(COALESCE(bc.insurance_claim_amount, 0)) as insurance_claim_amount , b.bill_type
FROM bill_charge bc
 JOIN bill b USING (bill_no)
 JOIN chargegroup_constants cgc ON cgc.chargegroup_id=bc.charge_group
WHERE b.bill_no = $P{billNo} AND bc.status!='X' AND bc.charge_head != 'BIDIS'
GROUP BY b.bill_no, b.bill_type, cgc.chargegroup_name, cgc.display_order
ORDER by b.bill_no, cgc.display_order]]>
	</queryString>
