	<queryString>
		<![CDATA[(SELECT TO_CHAR(pmsm.sale_date,'DD-MM-YYYY') AS open_Date,gd.dept_name,pmd.medicine_name,pmd.cust_item_code,mm.manf_name,
		sibd.batch_no,TO_CHAR(sibd.exp_dt,'DD-MM-YYYY') AS exp_date,case when pms.sale_unit = 'I' then pms.quantity else pms.quantity/pms.package_unit end AS qty,
		sic.control_type_name,
		pmsm.sale_id ,pmsm.type,pmsm.sale_id as bill_no,
		COALESCE(ptc.customer_name,pd.patient_name) AS patient_name, COALESCE(pmsm.doctor_name,'' ) AS doctor_name,
		case when pms.sale_unit = 'I' then issue_units else pmd.package_uom end as sale_unit, gd.center_id AS center_id, hcm.center_name
		FROM store_sales_main  pmsm
		JOIN store_sales_details pms ON (pms.sale_id=pmsm.sale_id)
		JOIN stores gd ON (pmsm.store_id=gd.dept_id)
		JOIN store_type_master sty ON (gd.store_type_id = sty.store_type_id)
		LEFT JOIN store_item_details pmd ON (pmd.medicine_id=pms.medicine_id)
		JOIN manf_master mm ON (pmd.manf_name=mm.manf_code)
		LEFT JOIN store_item_controltype sic ON (sic.control_type_id = pmd.control_type_id)
		JOIN store_item_batch_details sibd ON ( sibd.item_batch_id = pms.item_batch_id)
		JOIN bill b ON (b.bill_no=pmsm.bill_no)
		LEFT JOIN store_retail_customers ptc ON (ptc.customer_id = b.visit_id)
		LEFT JOIN patient_registration pr ON (pr.patient_id = b.visit_id)
		LEFT JOIN doctors d ON (d.doctor_id=pr.doctor)
		LEFT JOIN patient_details pd ON pr.mr_no = pd.mr_no
		LEFT JOIN hospital_center_master hcm ON (hcm.center_id = gd.center_id)
		WHERE  DATE_TRUNC('day',pmsm.sale_date)>=$P{fromDate} AND  DATE_TRUNC('day',pmsm.sale_date)<=$P{toDate}
		 AND ($X{IN, pmsm.store_id, store_idArray}) AND (gd.store_type_id:: character varying = $P{store_type_id}
		 OR  ''= $P{store_type_id}) AND (pmd.control_type_id:: character varying = $P{control_type_id}
		 OR ''= $P{control_type_id}) $P!{centerClause} )  ORDER BY dept_name,sale_id]]>
	</queryString>
