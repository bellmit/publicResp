		<![CDATA[SELECT DISTINCT  (CASE WHEN  payment_type='D' THEN  (SELECT doctor_name FROM  doctors WHERE  doctor_id=pd.payee_name) WHEN payment_type='R' THEN (SELECT doctor_name FROM   doctors WHERE doctor_id=pd.payee_name)
WHEN payment_type= 'F' THEN (SELECT referal_name FROM  referral where referal_no=pd.payee_name)
WHEN payment_type= 'O' THEN (SELECT oh_name FROM  outhouse_master where oh_id=pd.payee_name)
WHEN payment_type='C' THEN pd.payee_name ELSE   pd.payee_name END ) as payee_name  ,payee_name as payeeid, payment_type  ,sum(amount) as amount,  CASE WHEN payment_type='C' THEN 'Cash' WHEN payment_type='D' THEN 'Doctor Payments' WHEN  payment_type in ('R','F') THEN 'Refferal Payments' WHEN payment_type='O' THEN 'Out Tests'  END as paymentType, posted_date,pd.description,pd.category    FROM  payments_details pd   where voucher_no is null and pd.amount >0   $P!{paymentType} $P!{payeeId}  $P!{payDate}  GROUP BY  payee_name,payment_type,posted_date  ,pd.description,pd.category]]>
