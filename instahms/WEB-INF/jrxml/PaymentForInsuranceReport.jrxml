	</parameter>
	<parameter name="fromDate" class="java.util.Date" isForPrompting="false"/>
	<parameter name="toDate" class="java.util.Date" isForPrompting="false"/>
	<queryString>
		<![CDATA[select pdet.patient_name,p.payment_type,pdet.mr_no,claim_sent_date,claim_recd_date,
		tpa_id,tpa_name, (CASE WHEN  p.payment_type in('D','P','R') THEN  (SELECT doctor_name FROM doctors WHERE  doctor_id=p.payee_name)   WHEN p.payment_type= 'F' THEN (SELECT referal_name FROM referral WHERE referal_no=p.payee_name)  WHEN p.payment_type='O' THEN (SELECT oh_name FROM  outhouse_master where oh_id=p.payee_name)  ELSE p.payee_name END ) as payee_name,  
pd.description, pd.amount
FROM payments p 
JOIN (select pd.amount,pd.voucher_no,bill_no,pd.description,
pd.category from payments_details pd join bill_charge bc using(charge_id)) as pd using(voucher_no) 
JOIN bill b on b.bill_no=pd.bill_no join insurance_case ic on ic.bill_no=b.bill_no 
JOIN tpa_master t using(tpa_id) JOIN patient_details pdet on pdet.mr_no=ic.mr_no 
where date(p.date) < date(claim_recd_date)  and 
date(p.date) between $P{fromDate} and $P{toDate} $P!{payees}
order by  p.payee_name,tpa_id]]>
	</queryString>
