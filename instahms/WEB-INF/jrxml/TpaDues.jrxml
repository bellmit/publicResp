	<queryString>
		<![CDATA[SELECT pd.tpa_name, b.bill_no, b.status, finalized_date, patient_full_name, 
  b.total_claim, claim_recd_amount
FROM bill b
  JOIN patient_visit_details_ext_view pd ON (pd.patient_id = b.visit_id)
WHERE b.status != 'X' AND b.status != 'A' 
AND pd.tpa_id IS NOT NULL AND b.claim_status = 'S' And (total_claim - claim_recd_amount)>0 
  AND date(finalized_date) BETWEEN $P{fromDate} AND $P{toDate}]]>
	</queryString>
