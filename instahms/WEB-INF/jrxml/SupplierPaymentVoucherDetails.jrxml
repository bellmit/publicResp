	<parameter name="voucherNo" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[select voucher_no,pd.amount,pd.payee_name,posted_date,
coalesce(pi.invoice_no,pdn.debit_note_no,pdn.debit_note_no,'Direct Payment') as invoice_no,
coalesce(pi.supplier_id,pdn.supplier_id,pdn.supplier_id) AS supplier_id,pd.payment_id,sm.supplier_name,
pm.payment_mode, p.payment_mode_id, p.card_type_id, ctm.card_type,
reference_no,bank
FROM payments_details pd
JOIN payments p using (voucher_no)
LEFT JOIN store_invoice pi ON (pd.payment_id = pi.payment_id)
LEFT JOIN store_debit_note pdn ON (pdn.payment_id = pd.payment_id)
LEFT JOIN card_type_master ctm ON (ctm.card_type_id = p.card_type_id)
LEFT JOIN payment_mode_master pm ON (pm.mode_id = p.payment_mode_id)
JOIN supplier_master sm ON (sm.supplier_code = pd.payee_name)
WHERE voucher_no = $P{voucherNo}
order by payment_id]]>
