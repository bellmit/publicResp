	<queryString>
		<![CDATA[SELECT pdepo.receipt_id as deposit_no, pdepo.mr_no, pdepo.receipt_type as deposit_type, pdepo.amount, pdepo.created_at as deposit_date,c.counter_no as counter ,
       pdepo.payment_mode_id, pm.payment_mode, pdepo.card_type_id, ctm.card_type,
       pdepo.bank_name, pdepo.reference_no, pdepo.created_by AS username, pdepo.remarks
  FROM  receipt_usage_view pdepo
  LEFT JOIN counters c ON (c.counter_id = pdepo.counter)
  JOIN payment_mode_master pm ON (pdepo.payment_mode_id = pm.mode_id)
  LEFT JOIN card_type_master ctm ON (pdepo.card_type_id = ctm.card_Type_id)
  WHERE mr_no = $P{mr_no}]]>
