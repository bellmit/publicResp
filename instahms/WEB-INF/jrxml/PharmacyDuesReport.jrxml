	<queryString>
		<![CDATA[SELECT b.bill_no,open_date,bill_type,SUM(bc.amount) AS amount,SUM(bc.discount) AS discount,b.visit_id,
			COALESCE(pd.patient_name,prc.customer_name) AS patname,pr.mr_no,
			COALESCE((SELECT coalesce(sum(br.total_receipts),0) FROM
					bill br WHERE br.bill_no = b.bill_no GROUP BY bill_no),0) AS receipts,b.visit_type,
CASE WHEN b.bill_type='P' THEN 'Bill Now Bills'
						WHEN (b.bill_type='C' AND b.visit_type='r') THEN 'Retail Credit Bills'
						ELSE 'Bill Later Bills' END as billtype
		FROM store_sales_main ssm
		JOIN bill b using(bill_no)
		JOIN bill_charge bc ON(ssm.charge_id = bc.charge_id)
		LEFT JOIN patient_registration pr ON (pr.patient_id=b.visit_id)
		LEFT JOIN patient_details pd ON (pr.mr_no=pd.mr_no)
		LEFT JOIN store_retail_customers prc ON (prc.customer_id=b.visit_id)

		WHERE ((bill_type='M') OR (bill_type='C' ))  AND b.status = 'A'
 AND date(open_date) BETWEEN $P{fromDate} AND $P{toDate}
AND ( ($P{mrno}='' OR $P{mrno}=pr.mr_no) OR ($P{mrno}='' OR $P{mrno}=COALESCE(pd.patient_name,prc.customer_name)))
		GROUP BY b.bill_no, open_date, bill_type, b.visit_id,pd.patient_name,prc.customer_name, pr.mr_no,b.visit_type,billtype
		ORDER BY billtype DESC ,bill_no]]>
	</queryString>
