	<queryString>
		<![CDATA[SELECT br.bill_no, 
		CASE WHEN r.receipt_type='R'
AND r.tpa_id IS NOT NULL THEN 'S' ELSE r.receipt_type END as payment_type, r.receipt_type, br.receipt_no, r.display_date, r.amount,
r.payment_mode_id, r.bank_name, r.reference_no, r.remarks
FROM receipts r
JOIN bill_receipts br ON(r.receipt_id = br.receipt_no)
JOIN bill b ON (br.bill_no = b.bill_no)
LEFT JOIN counters c ON (c.counter_id = r.counter)
WHERE b.visit_id = $P{patientId} AND (restriction_type = 'P' OR c.counter_type='P') AND r.receipt_type='R'
	AND r.tpa_id IS  NULL]]>
	</queryString>
