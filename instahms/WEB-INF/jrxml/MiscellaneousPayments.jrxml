	<queryString>
		<![CDATA[SELECT CASE WHEN pd.payment_type='C' THEN 'Cash Voucher' WHEN pd.payment_type='D' THEN 
'Doctor Payments' WHEN pd.payment_type in ('R','F') THEN 'Referral Doctor Payments' WHEN pd.payment_type='O' 
THEN 'Out Hospital Payments' END as payment_type,p.voucher_no,
p.type,pd.amount, p.tax_type,p.tax_amount,p.date,p.counter,p.username,p.reference_no,p.tds_amount,  
CASE WHEN pd.payment_type='D' THEN (SELECT doctor_name from doctors where  doctor_id=p.payee_name)
WHEN pd.payment_type='R' THEN (SELECT doctor_name from doctors where doctor_id=p.payee_name) 
WHEN pd.payment_type='F' THEN (SELECT referal_name from referral where referal_no=p.payee_name) 
WHEN pd.payment_type='O' THEN (SELECT oh_name FROM  outhouse_master where oh_id=p.payee_name) 
ELSE p.payee_name END as payee_name,  CASE WHEN p.mode='C' THEN 'CASH' WHEN p.mode='R' 
THEN 'Credit Card' WHEN p.mode='B' THEN  'Debit Card' WHEN p.mode='Q' THEN 'Cheque' 
ELSE 'DD' END as mode ,p.bank ,pd.payment_type,pd.description,pd.category  
FROM payments p join payments_details pd  on (p.payee_name=pd.payee_name and 
pd.voucher_no=p.voucher_no )  where pd.voucher_no is not null  $P!{paymentType}  $P!{payeeId}  $P!{payDate} 
order by pd.payment_type]]>
	</queryString>
